#define EXTRAS_SHARED (
   (change_outfit
      ($async $loading)
      {if $p9director
         {$p9director select_extras $this}
      }
   )
   (on_outfit_loaded
      ($cat $dir)
      {$dir set_name $cat}
      {$dir set_showing FALSE}
      {do
         ($proxy {find_obj {$this dir} $cat})
         {if $proxy
            {$proxy set_showing FALSE}
            {$proxy copy_bounding_sphere $dir}
         }
      }
   )
)
