{new UIPanel
   garage_vehiclepanel
   (file "../tour_world/tour_world_vehicle.milo")
   (focus_on_enter "BandButton_01.btn")
   (current_vehicle_sound 0)
   (vehicle_volume -20)
   (is_really_up FALSE)
   (spark_index 0)
   (current_focus FALSE)
   (trigger_anims FALSE)
   (i_am_wet FALSE)
   (enter_focus_done FALSE)
   (mapping
      (
         (BandButton_01.btn
            veh_camper_van
            "campervan"
            van_idle_loop.cue
            z_campervan
            campervan_purchased_screen
            campervan_purchased_vehicle_panel
            BandButton_02.btn
         )
         (BandButton_02.btn
            veh_minibus
            "minibus"
            minibus_idle_loop.cue
            z_minibus
            minibus_purchased_screen
            minibus_purchased_vehicle_panel
            BandButton_03.btn
         )
         (BandButton_03.btn
            veh_boat
            "boat"
            boat_idle_loop.cue
            z_boat
            boat_purchased_screen
            boat_purchased_vehicle_panel
            BandButton_04.btn
         )
         (BandButton_04.btn
            veh_helicopter
            "helicopter"
            heli_idle_loop.cue
            z_helicopter
            helicopter_purchased_screen
            helicopter_purchased_vehicle_panel
            BandButton_05.btn
         )
         (BandButton_05.btn
            veh_tourbus
            "tourbus"
            bus_idle_loop.cue
            z_tourbus
            tourbus_purchased_screen
            tourbus_purchased_vehicle_panel
            BandButton_06.btn
         )
         (BandButton_06.btn
            veh_hovercraft
            "hovercraft"
            hovercraft_idle_loop.cue
            z_hovercraft
            hovercraft_purchased_screen
            hovercraft_purchased_vehicle_panel
            BandButton_07.btn
         )
         (BandButton_07.btn
            veh_submarine
            "submarine"
            sub_idle_loop.cue
            z_submarine
            submarine_purchased_screen
            submarine_purchased_vehicle_panel
            BandButton_08.btn
         )
         (BandButton_08.btn
            veh_private_jet
            "jet"
            jet_idle_loop.cue
            jet
            jet_purchased_screen
            jet_purchased_vehicle_panel
            BandButton_09.btn
         )
         (BandButton_09.btn
            veh_teleporter
            "teleporter"
            teleport_loop.cue
            z_teleport
            teleporter_purchased_screen
            teleporter_purchased_vehicle_panel
            0
         )
      )
   )
   (venues
      (ven_crystal_mine
         ven_bill_ditt_construction_co
         ven_blockton_manor
         ven_the_jolly_rocker
         ven_the_parrot_n_pegleg
         ven_lego_city_rooftop
         ven_the_cavern
         ven_stud_farm
         ven_sub_zero_research_facility
         ven_the_abyss
         ven_blockingham_castle
         ven_wembrick_stadium
         ven_the_moon
         ven_lego_city_zoo
         ven_volcanic_rock
         ven_wreck_n_roll_bowl
         ven_space_rock_festival
         ven_ufo
      )
   )
   (do_teleporter_spark
      {do
         ($vehicle_name {elem [current_focus] 1})
         {if {== [is_really_up] TRUE}
            {if {== $vehicle_name veh_teleporter}
               {do
                  ($trigger {{mixergarage_panel loaded_dir} find "teleporter_flash.trig"})
                  {$trigger trigger}
                  {if_else {== [spark_index] 0}
                     {do
                        {synth play tele_spark1.cue}
                        {$this set spark_index 1}
                     }
                     {do
                        {synth play tele_spark2.cue}
                        {$this set spark_index 0}
                     }
                  }
                  {script_task kTaskUISeconds
                     (script {$this do_teleporter_spark})
                     (once TRUE)
                     (delay 3)
                  }
               }
            }
         }
      }
   )
   (enter
      {do
         ($focus {$this find [focus_on_enter]})
         {BandButton_01.btn set_showing 0}
         {challenge_mgr clear_current_challenge}
         {garage_vehiclescreen start_camera}
         {$this set_focus $focus}
         {$this focus_vehicle $focus FALSE}
         {content_mgr start_refresh}
         {$this set enter_focus_done TRUE}
      }
      {if_else {{tour band} is_new_vehicle_unlocked}
         {do
            {new_vehicle_panel set vehicle {{tour band} get_new_unlocked_vehicle}}
            {ui push_screen new_vehicle_screen}
         }
         {do
            {$this set_vehicles_showing}
            {Information.grp set_showing FALSE}
            {foreach_int $i 0 {size [venues]}
               {do
                  ($ven {elem [venues] $i})
                  {if_else {== $ven ven_bill_ditt_construction_co}
                     {if
                        {&&
                           {{tour band} is_venue_new $ven}
                           {> {{tour band} get_stars_at_venue ven_lego_city_central} 5}
                        }
                        {set $hint {symbol {sprint "hint_" $ven}}}
                        {unless {{tour band} have_seen_hint $hint}
                           {new_venue_panel set venue $ven}
                           {ui push_screen new_venue_screen}
                           {set $i {size [venues]}}
                        }
                     }
                     {if {{tour band} is_venue_new $ven}
                        {set $hint {symbol {sprint "hint_" $ven}}}
                        {unless {{tour band} have_seen_hint $hint}
                           {new_venue_panel set venue $ven}
                           {ui push_screen new_venue_screen}
                           {set $i {size [venues]}}
                        }
                     }
                  }
               }
            }
         }
      }
      {$this set_vehicles_showing}
      {input_mgr set_limit kLimitUILeader}
      {$this set is_really_up TRUE}
   )
   (stop_veh_sound
      {if [current_vehicle_sound]
         {print "stopping vehicle sound..."}
         {synth stop [current_vehicle_sound]}
         {$this set current_vehicle_sound 0}
      }
      {if [i_am_wet]
         {synth stop water_loop.cue}
         {$this set i_am_wet FALSE}
      }
   )
   (exit
      {input_mgr set_limit kLimitUILeaderNoSwap}
      {$this set enter_focus_done FALSE}
   )
   (exit_complete {$this set is_really_up FALSE})
   (poll)
   (set_vehicles_showing
      #ifndef DAN_FAST
      {$this set_rdir_showing "BandButton_01.btn"}
      {$this set_rdir_showing "BandButton_02.btn"}
      {$this set_rdir_showing "BandButton_03.btn"}
      {$this set_rdir_showing "BandButton_04.btn"}
      {$this set_rdir_showing "BandButton_05.btn"}
      {$this set_rdir_showing "BandButton_06.btn"}
      {$this set_rdir_showing "BandButton_07.btn"}
      {$this set_rdir_showing "BandButton_08.btn"}
      {$this set_rdir_showing "BandButton_09.btn"}
      #endif
   )
   (set_rdir_showing
      ($name)
      {do
         ($button {find_exists [mapping] {symbol $name}})
         {if_else {{tour band} is_vehicle_purchased {elem $button 1}}
            {{mixergarage_panel find {elem $button 4}} set_showing 1}
            {{mixergarage_panel find {elem $button 4}} set_showing 0}
         }
      }
   )
   (purchased_vehicle_screens
      (
         (minibus_purchased_screen)
         (boat_purchased_screen)
         (helicopter_purchased_screen)
         (tourbus_purchased_screen)
         (hovercraft_purchased_screen)
         (submarine_purchased_screen)
         (jet_purchased_screen)
         (teleporter_purchased_screen)
      )
   )
   (make_me_wet
      {do
         ($vehicle_name {elem [current_focus] 1})
         {if {! [i_am_wet]}
            {if {'||' {== $vehicle_name veh_submarine} {== $vehicle_name veh_boat}}
               {synth play water_loop.cue}
               {$this set i_am_wet TRUE}
            }
         }
      }
   )
   (play_veh_sound
      {do
         {if [current_focus]
            {do
               ($sound {elem [current_focus] 3})
               {$this stop_veh_sound}
               {synth play $sound (volume [vehicle_volume])}
               {$this set current_vehicle_sound $sound}
               {$this make_me_wet}
            }
         }
      }
   )
   (TRANSITION_COMPLETE_MSG
      {if_else
         {!=
            {find_exists [purchased_vehicle_screens] {symbol {$old_screen name}}}
            kDataUnhandled
         }
         {do
            ($button {find_exists [mapping] {symbol {$this focus_name}}})
            ($sound {elem $button 3})
            {$this set_vehicles_showing}
            {Information.grp set_showing FALSE}
            {foreach_int $i 0 {size [venues]}
               {do
                  ($ven {elem [venues] $i})
                  {if_else {== $ven ven_bill_ditt_construction_co}
                     {if
                        {&&
                           {{tour band} is_venue_new $ven}
                           {> {{tour band} get_stars_at_venue ven_lego_city_central} 5}
                        }
                        {set $hint {symbol {sprint "hint_" $ven}}}
                        {unless {{tour band} have_seen_hint $hint}
                           {new_venue_panel set venue $ven}
                           {ui push_screen new_venue_screen}
                           {set $i {size [venues]}}
                        }
                     }
                     {if {{tour band} is_venue_new $ven}
                        {set $hint {symbol {sprint "hint_" $ven}}}
                        {unless {{tour band} have_seen_hint $hint}
                           {new_venue_panel set venue $ven}
                           {ui push_screen new_venue_screen}
                           {set $i {size [venues]}}
                        }
                     }
                  }
               }
            }
            {$this play_veh_sound}
         }
         {unless
            {'||'
               {&&
                  {== $old_screen new_vehicle_screen}
                  {== {new_vehicle_panel get vehicle} veh_helicopter}
               }
               {&& {{tour band} is_manager_hired} {show_hint hint_manager_hired ''}}
            }
            {do
               ($focus {$this find [focus_on_enter]})
               {$this set_focus $focus}
               {garage_vehiclescreen start_camera}
               {$this focus_vehicle $focus FALSE}
            }
         }
      }
   )
   (FOCUS_MSG
      {if [enter_focus_done]
         {synth play move_hub.cue}
         {$this focus_vehicle $new_focus $old_focus}
      }
      kDataUnhandled
   )
   (vehicle_name
      {do
         ($button {find_exists [mapping] {symbol {$this focus_name}}})
         {elem $button 1}
      }
   )
   (BUTTON_DOWN_MSG
      {switch $action
         (kAction_Confirm
            {if_else {$this is_vehicle_purchased}
               {do
                  ($band {tour band})
                  {if {$band is_vehicle_new {$this vehicle_name}}
                     {$band set_vehicle_age {$this vehicle_name} FALSE}
                  }
                  {ui goto_screen garage_venuescreen}
                  {synth play button_select}
               }
               {do
                  ($button {find_exists [mapping] {symbol {$this focus_name}}})
                  ($vehicle {tour get_vehicle {elem $button 1}})
                  ($band {tour band})
                  {synth play button_select}
                  {{tour band} purchase_vehicle $vehicle}
                  {$this update_helpbar {$this get_focus}}
               }
            }
         )
         (kAction_Cancel
            {synth play button_back}
            {$this stop_veh_sound}
            {ui goto_screen loading_hub_screen}
            {{tour band} clear_vehicle}
            {fx fade_out}
         )
         #ifndef HX_WII
         (kAction_Option
            {if {$this is_vehicle_purchased}
               {do
                  ($button {find_exists [mapping] {symbol {$this focus_name}}})
                  ($screen {elem $button 5})
                  ($panel {elem $button 6})
                  {$panel set purchasing FALSE}
                  {ui push_screen $screen}
                  {synth play button_select}
               }
            }
         )
         #endif
         (kAction_Up
            {if_else {&& {$this can_move_up} {== {garage_vehiclescreen check_shot_ended} 1}}
               {do
                  {$this set trigger_anims TRUE}
                  kDataUnhandled
               }
               FALSE
            }
         )
         (kAction_Down
            {if_else {&& {$this can_move_down} {== {garage_vehiclescreen check_shot_ended} 1}}
               {do
                  {$this set trigger_anims TRUE}
                  kDataUnhandled
               }
               FALSE
            }
         )
         (kAction_Left
            {if_else {&& {$this can_move_up} {== {garage_vehiclescreen check_shot_ended} 1}}
               {do
                  {$this set trigger_anims TRUE}
                  kDataUnhandled
               }
               FALSE
            }
         )
         (kAction_Right
            {if_else {&& {$this can_move_down} {== {garage_vehiclescreen check_shot_ended} 1}}
               {do
                  {$this set trigger_anims TRUE}
                  kDataUnhandled
               }
               FALSE
            }
         )
      }
   )
   (update_helpbar
      ($newfocus)
      {do
         ($button {find_exists [mapping] {symbol {$newfocus name}}})
         #ifndef HX_WII
         {if_else {{tour band} is_vehicle_purchased {elem $button 1}}
            {if_else {> {user_mgr get_num_participants} 1}
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm helpbar_confirm)
                     (option view_vehicle)
                     (state_confirm helpbar_take_control)
                  )
               }
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm helpbar_confirm)
                     (option view_vehicle)
                  )
               }
            }
            {if_else {> {user_mgr get_num_participants} 1}
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm store_buy_now)
                     (state_confirm helpbar_take_control)
                  )
               }
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm store_buy_now)
                  )
               }
            }
         }
         #else
         {if_else {{tour band} is_vehicle_purchased {elem $button 1}}
            {if_else {> {user_mgr get_num_participants} 1}
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm helpbar_confirm)
                     (state_confirm helpbar_take_control)
                  )
               }
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm helpbar_confirm)
                  )
               }
            }
            {if_else {> {user_mgr get_num_participants} 1}
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm store_buy_now)
                     (state_confirm helpbar_take_control)
                  )
               }
               {helpbar set_config
                  (
                     (cancel helpbar_back)
                     (confirm store_buy_now)
                  )
               }
            }
         }
         #endif
      }
   )
   (is_vehicle_unlocked
      ($button_name)
      {do
         ($button {find_exists [mapping] {symbol $button_name}})
         ($vehicle {tour get_vehicle {elem $button 1}})
         {$vehicle is_unlocked}
      }
   )
   (is_vehicle_purchased
      {do
         ($button {find_exists [mapping] {symbol {$this focus_name}}})
         {{tour band} is_vehicle_purchased {elem $button 1}}
      }
   )
   (can_move_down
      {if_else {!= {symbol {$this focus_name}} BandButton_01.btn}
         {do
            ($nav_down {{$this get_focus} get nav_down})
            {$this is_vehicle_unlocked {$nav_down name}}
         }
         FALSE
      }
   )
   (can_move_up
      {if_else {!= {symbol {$this focus_name}} BandButton_09.btn}
         {do
            ($button {find_exists [mapping] {symbol {$this focus_name}}})
            ($nav_up {elem $button 7})
            {$this is_vehicle_unlocked $nav_up}
         }
         FALSE
      }
   )
   (focus_vehicle
      ($newfocus $oldfocus)
      {do
         ($button {find_exists [mapping] {symbol {$newfocus name}}})
         {$newfocus set_showing TRUE}
         {if $oldfocus
            {$oldfocus set_showing FALSE}
         }
         {$this set focus_on_enter {$newfocus name}}
         {$this stop_veh_sound}
         {if $button
            {do
               ($vehicle_name {elem $button 1})
               ($camera {elem $button 2})
               ($sound {elem $button 3})
               {$this set current_focus $button}
               {if {$this is_vehicle_unlocked {elem $button 0}}
                  {do
                     {$this set_current_vehicle $vehicle_name}
                     {garage_vehiclescreen goto_camera $camera}
                     {garage_gigmenu set focus_on_enter $camera}
                     {garage_venuepanel set focus_on_enter $camera}
                     {if {{tour band} is_vehicle_purchased {elem $button 1}}
                        {$this play_veh_sound}
                     }
                     #ifndef DAN_FAST
                     {if {== [trigger_anims] TRUE}
                        {do
                           ($stopall {{mixergarage_panel loaded_dir} find "Stop_All_Vehicles.trig"})
                           ($trigger
                              {{mixergarage_panel loaded_dir} find {sprint $vehicle_name ".trig"}}
                           )
                           {$stopall trigger}
                           {$trigger trigger}
                           {if {== $vehicle_name veh_teleporter}
                              {if {{tour band} is_vehicle_purchased {elem $button 1}}
                                 {$this do_teleporter_spark}
                              }
                           }
                           {$this set trigger_anims FALSE}
                        }
                     }
                     #endif
                     {$this update_helpbar $newfocus}
                  }
               }
               {$this make_me_wet}
            }
         }
      }
   )
   (set_current_vehicle
      ($vehicle_name)
      {if {gamemode get is_tour}
         {do
            ($vehicle {tour get_vehicle $vehicle_name})
            ($band {tour band})
            {$band set_vehicle $vehicle}
            {info.lbl set text_token {$vehicle get_description}}
            {if_else {$band is_vehicle_purchased $vehicle_name}
               {Information.grp set_showing FALSE}
               {do
                  {cost.lbl set text_token {$vehicle get_formatted_cost}}
                  {Information.grp set_showing TRUE}
               }
            }
         }
      }
   )
}