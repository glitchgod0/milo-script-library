(get_float_key
   ($float_val)
   {do
      ($parent $this)
      {milo dialog
         'Set Value'
         (editor
            (value float (post_sync {$this do_callback FALSE}))
            (reset
               script
               (script
                  {if {exists milo}
                     {set [value] [initial_val]}
                     {$this do_callback FALSE}
                     {set [changed] FALSE}
                  }
               )
            )
         )
         (value $float_val)
         (initial_val 0)
         (dialog_width 230)
         (parent "")
         (changed FALSE)
         (enter
            {set [initial_val] $float_val}
            {set [parent] $parent}
         )
         (exit
            {if [changed]
               {[parent] set_hilight_val [initial_val] FALSE}
               {$this do_callback TRUE}
            }
         )
         (do_callback
            ($record)
            {if {! $record}
               {set [changed] TRUE}
            }
            {[parent] set_hilight_val [value] $record}
         )
      }
   }
)
(get_3_floats
   ($in_x $in_y $in_z)
   {do
      ($parent $this)
      {milo dialog
         'Set Value'
         (editor
            (x float (post_sync {$this do_callback FALSE}))
            (y float (post_sync {$this do_callback FALSE}))
            (z float (post_sync {$this do_callback FALSE}))
            (reset
               script
               (script
                  {if {exists milo}
                     {$this reset}
                  }
               )
            )
         )
         (x $in_x)
         (y $in_y)
         (z $in_z)
         (in_x 0)
         (in_y 0)
         (in_z 0)
         (dialog_width 230)
         (parent "")
         (changed FALSE)
         (enter
            {set [in_x] $in_x}
            {set [in_y] $in_y}
            {set [in_z] $in_z}
            {set [parent] $parent}
            {$this reset}
         )
         (exit
            {if [changed]
               {[parent] set_hilight_val [in_x] [in_y] [in_z] FALSE}
               {$this do_callback TRUE}
            }
         )
         (do_callback
            ($record)
            {if {! $record}
               {set [changed] TRUE}
            }
            {[parent] set_hilight_val [x] [y] [z] $record}
         )
         (reset
            {set [x] [in_x]}
            {set [y] [in_y]}
            {set [z] [in_z]}
            {$this do_callback FALSE}
            {set [changed] FALSE}
         )
      }
   }
)