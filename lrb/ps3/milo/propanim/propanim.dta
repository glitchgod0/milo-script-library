(set_scale_move
   ($scale)
   {$this set scale_move $scale}
   {if_else $scale
      {do
         {$this set_button_icon "Tranlate Keys Mode" "milo/propanim/Translate.ico"}
         {$this set_button_icon "Scale Keys Mode" "milo/propanim/Scale_Toggle.ico"}
      }
      {do
         {$this set_button_icon
            "Tranlate Keys Mode"
            "milo/propanim/Translate_Toggle.ico"
         }
         {$this set_button_icon "Scale Keys Mode" "milo/propanim/Scale.ico"}
      }
   }
)
