{set $tex_mem 0}
{set $mesh_mem 0}
{{milo cur_dir} iterate Tex $tex {'+=' $tex_mem {$tex size_kb}}}
{{milo cur_dir} iterate
   Mesh
   $mesh
   {'+=' $mesh_mem {'*' {$mesh num_verts} 80}}
   {'+=' $mesh_mem {'*' {$mesh num_faces} 6}}
}
{set $mesh_mem {int {/ $mesh_mem 1024}}}
{notify "Texture size: " $tex_mem " kb\nMesh size: " $mesh_mem " kb"}