; COLOR MACROS
; TODO fix so loops are 1920 long

[OFF                   (range 1920 1920)   ]
[BLACK                 (range 5000 5000)   ]
[YELLOW                (range 100 100)         ]
[MEDIUMCLUB_YELLOW    (range 2 2)         ]
[RED                   (range 2021 2021)   ]
[BLUE 	              (range 3941 3941)   ]
[GREEN                 (range 5861 5861)   ]
[ORANGE                (range 7781 7781)   ]
[PURPLE                (range 9701 9701)   ]
[PINK                  (range 11621 11621) ]
[BABY_BLUE             (range 13541 13541) ]
[COOL_WHITE            (range 15461 15461) ]
[DIM_YELLOW                (range 960 960)         ]
[DIM_RED                   (range 2880 2880)   ]
[DIM_BLUE 	           (range 4800 4800)   ]
[DIM_GREEN                 (range 6720 6720)   ]
[DIM_ORANGE                (range 8640 8640)   ]
[DIM_PURPLE                (range 10560 10560)  ]
[DIM_PINK                  (range 12480 12480) ]
[DIM_BABY_BLUE             (range 14400 14400) ]
[DIM_COOL_WHITE            (range 16320 16320) ]
[DIM_YELLOW_BLINK          (loop 960 1920)]
[DIM_RED_BLINK             (loop 2880 3840)]
[DIM_BLUE_BLINK            (loop 4800 5760)]
[DIM_GREEN_BLINK           (loop 6720 7680)]
[DIM_ORANGE_BLINK          (loop 8640 9600)]
[DIM_PURPLE_BLINK          (loop 10560 11520)]
[DIM_PINK_BLINK            (loop 12480 13440)]
[DIM_BABY_BLUE_BLINK       (loop 14400 15360)]
[DIM_COOL_WHITE_BLINK      (loop 16320 17280)]
[YELLOW_BLINK          (loop 1 1920)]
[RED_BLINK             (loop 1921 3840)]
[BLUE_BLINK            (loop 3841 5760)]
[GREEN_BLINK           (loop 5761 7680)]
[ORANGE_BLINK          (loop 7681 9600)]
[PURPLE_BLINK          (loop 9601 11520)]
[PINK_BLINK            (loop 11521 13440)]
[BABY_BLUE_BLINK       (loop 13441 15360)]
[COOL_WHITE_BLINK      (loop 15361 16280)]
[YELLOW_RED            (loop 1 3840)]
[RED_BLUE              (loop 1921 5760)]
[BLUE_GREEN            (loop 3841 7680)]
[GREEN_ORANGE          (loop 5761 9600)]
[ORANGE_PURPLE         (loop 7681 11520)]
[PURPLE_PINK           (loop 9601 13440)]
[PINK_BABY_BLUE        (loop 11521 15360)]
[BABY_BLUE_COOL_WHITE  (loop 13441 16280)]
[PARTY_DIM_RED                   (range 16281 16281)   ]
[CROWD_LIT
      {venue switch_anim crowd_l.litanim    (range 0 0)   (blend $blend)}
      {venue switch_anim crowd_r.litanim    (range 0 0)   (blend $blend)}
      {venue switch_anim crowd_bck.litanim  (range 0 0)   (blend $blend)}
 ]
 [CROWD_DARK
      {venue switch_anim crowd_l.litanim    (range 1 1)   (blend $blend)}
      {venue switch_anim crowd_r.litanim    (range 1 1)   (blend $blend)}
      {venue switch_anim crowd_bck.litanim  (range 1 1)   (blend $blend)}
 ]
 [CHAR_LIT
      {venue switch_anim char_l.litanim    (range 0 0)   (blend $blend)}
      {venue switch_anim char_r.litanim    (range 0 0)   (blend $blend)}
      {if {== $arena.excitement kExcitementPeak}
         {if {== $arena.singer_gender kGenderMale}
            {venue switch_anim char_bck.litanim  (range 1000 1000) (blend $blend)}
            {venue switch_anim char_bck.litanim  (range 2000 2000) (blend $blend)}
         }
         {venue switch_anim char_bck.litanim  (range 0 0) (blend $blend)}
      }
 ]
  [CHAR_DARK
      {venue switch_anim char_l.litanim    (range 1 1)   (blend $blend)}
      {venue switch_anim char_r.litanim    (range 1 1)   (blend $blend)}
      {venue switch_anim char_bck.litanim  (range 1 1)   (blend $blend)}
 ]
 [CHAR_OFF
      {venue switch_anim char_l.litanim    (range 2 2)   (blend 0)}
      {venue switch_anim char_r.litanim    (range 2 2)   (blend 0)}
      {venue switch_anim char_bck.litanim  (range 2 2)   (blend 0)}
      {floorspot_char.mesh set_showing FALSE}
 ]
  [STAGE_LIT
      {venue switch_anim stg_l.litanim    (range 0 0)   (blend $blend)}
      {venue switch_anim stg_r.litanim    (range 0 0)   (blend $blend)}
      {venue switch_anim stg_bck.litanim  (range 0 0)   (blend $blend)}
 ]
 [STAGE_DARK
      {venue switch_anim stg_l.litanim    (range 1 1)   (blend $blend)}
      {venue switch_anim stg_r.litanim    (range 1 1)   (blend $blend)}
      {venue switch_anim stg_bck.litanim  (range 1 1)   (blend $blend)}
 ]

                        

