(init
   {defun interp_scale (args $anim $scale $blend)
      {interp_task $blend 
         (interp $s {$anim anim_scale 0} $scale) 
         (preserve $anim)
         {$anim set_scale_offset 0 (auto_scale $s) (blend 0)}
      }
   }

   ; resets the floorspot for after you lose

   {floorspot_char.mesh set_showing TRUE}

   ; GROUP COLOR FUNCTIONS

   {defun color_good_blue (args $blend) 
      {venue_color_good_blue}
      {venue switch_anim spot_A.envanim    BLUE                 (blend $blend)}
      {venue switch_anim spot_B.envanim    BLUE_BLINK (scale .25) (blend $blend)}
      {venue switch_anim spot_C.envanim    BABY_BLUE            (blend $blend)}
      {venue switch_anim spot_D.envanim    PURPLE               (blend $blend)}
      {venue switch_anim spot_E.envanim    COOL_WHITE           (blend $blend)}
      {venue switch_anim spot_F.envanim    PURPLE               (blend $blend)}        
      {venue switch_anim fresnel_A.envanim BLUE                 (blend $blend)}
      {venue switch_anim fresnel_B.envanim BLUE                 (blend $blend)}
      {venue switch_anim fresnel_C.envanim BLUE                 (blend $blend)}
      {venue switch_anim fresnel_D.envanim PURPLE               (blend $blend)}
      {venue switch_anim venue.envanim     BLUE                 (blend $blend)}
      {venue switch_anim venue_stg.envanim     BLUE                 (blend $blend)}
      {venue switch_anim venue_crowd.envanim     BLUE                 (blend $blend)}
       CROWD_LIT
      STAGE_LIT
      CHAR_LIT
   }
   {defun color_ok_blue (args $blend) 
      {venue_color_ok_blue}
      {venue switch_anim spot_A.envanim    DIM_BLUE                 (blend $blend)}
      {venue switch_anim spot_B.envanim    BLUE_BLINK (scale .25) (blend $blend)}
      {venue switch_anim spot_C.envanim    OFF            (blend $blend)}
      {venue switch_anim spot_D.envanim    DIM_PURPLE               (blend $blend)}
      {venue switch_anim spot_E.envanim    DIM_COOL_WHITE           (blend $blend)}
      {venue switch_anim spot_F.envanim    PURPLE               (blend $blend)}        
      {venue switch_anim fresnel_A.envanim DIM_BLUE                 (blend $blend)}
      {venue switch_anim fresnel_B.envanim DIM_BLUE                 (blend $blend)}
      {venue switch_anim fresnel_C.envanim OFF                 (blend $blend)}
      {venue switch_anim fresnel_D.envanim DIM_PURPLE               (blend $blend)}
      {venue switch_anim venue.envanim     DIM_BLUE                 (blend $blend)}
      {venue switch_anim venue.envanim     DIM_BLUE                 (blend $blend)}
      {venue switch_anim venue.envanim     DIM_BLUE                 (blend $blend)}
      {venue switch_anim venue_stg.envanim     DIM_BLUE                 (blend $blend)}
      {venue switch_anim venue_crowd.envanim     DIM_BLUE                 (blend $blend)}
      CROWD_LIT
      STAGE_LIT
      CHAR_LIT
   }

