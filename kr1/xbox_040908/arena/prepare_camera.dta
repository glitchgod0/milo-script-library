   {venue switch_cam Cam_char_r   "Right char 10"
      (start 1920) (end 1920)             (filter .2) (fade $camera.fade)}
)
]

; Pool of stage shots
[STAGE_SHOTS
(
   {venue switch_cam Cam_swoop   "Stage 0"  (mediumclub 0)
      (start 960) (end 1920) (scale .2)   (filter .05) (fade $camera.fade)}

   {venue switch_cam Cam_swoop   "Stage 1" (mediumclub 0) (tvstudio 0)
      (start 0) (end 1920) (scale .2)     (filter .05) (fade $camera.fade)}
     
   {venue switch_cam Cam_swoop2   "Stage 2"  (subway 1) (stadium 6) (party 1)
      (start 960) (end 1920) (scale .2)   (filter .05) (fade $camera.fade)}
        
   {venue switch_cam Cam_swoop3   "Stage 4"    (stadium 0) (party 0) (subway 0)
      (start 960) (end 1920) (scale .2)   (filter .05) (fade $camera.fade)}

   {venue switch_cam Cam_swoop3   "Stage 5" (party 1)
      (start 0) (end 1920) (scale .3)     (filter .05) (fade $camera.fade)}

   {venue switch_cam Cam_swoop    "Stage 6" (party 0) (tvstudio 0) (stadium 0)
      (start 480) (end 980) (scale .05)   (filter .05) (fade $camera.fade)}

   {venue switch_cam Cam_zoom_in  "Stage 7" (statefair 0) (stadium 3) (party 0) (subway 0) (mediumclub 0)
      (start 480) (end 860) (scale .02)              (filter .05) (fade $camera.fade)}

   {venue switch_cam Cam_zoom_in  "Stage 9" (party 0) (stadium 0) (mediumclub 1) (theater 0) (eyes kEyeCamera)
      (start 960) (end 1440) (scale .02)             (filter .05) (fade $camera.fade)}

   {venue switch_cam Cam_zoom_out "Stage 13" (eyes kEyeCamera) (mediumclub 0) (theater 0)
      (scale 0.26)                        (filter .05) (fade $camera.fade)}
  


)
]

