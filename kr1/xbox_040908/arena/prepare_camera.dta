; Beginning and end camera
[CAM_GUITAR {venue switch_cam Cam_guitarist "Guitar" (scale 0.2) (fade $camera.fade)}] 
[CAM_KEYBOARD {venue switch_cam Cam_keyboardist "Keyboard" (scale 0.2) (fade $camera.fade)}] 
[CAM_BEGIN {venue switch_cam Cam_setup "Begin" (party 1) (scale 0.2) (fade $camera.fade)}] 
[CAM_WIN   {venue switch_cam Cam_close "Win" (tvstudio 0) (statefair 0) (divebar 0) (subway 0) (theater 0) (start 0) (end 1920) (scale .5) (fade $camera.fade)}] 
[CAM_LOUSY {venue switch_cam Cam_zoom_in "Lousy" (tvstudio 0) (statefair 0) (divebar 0) (subway 0) (theater 0) (stadium 0) (party 0) (mediumclub 0) (end 0) (fade $camera.fade)}] 
[CAM_LOSE {venue switch_cam Cam_zoom_in "Lose" (start 0) (end 1920) (fade $camera.fade)}]

; Pool of left character shots
[L_CHAR_SHOTS
(
   {venue switch_cam Cam_char_360 "Left char 0"
      (start 50) (end 150) (scale .02) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_360 "Left char 1"
      (start 150) (end 050) (scale -.02) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_360 "Left char 2"
      (start 150) (end 150) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_360 "Left char 3"
      (end 0) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_circle_wide "Left char 4"
      (start 50) (end 150) (scale .02) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_circle_wide "Left char 5"
      (start 150) (end 50) (scale -.02)(filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_circle_wide "Left char 6"
      (end 0) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_l "Left char 7"
      (start 0) (end 900) (scale .15) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_l "Left char 8"
      (start 900) (end 0) (scale -.15)    (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_l   "Left char 9"
      (start 860) (end 860) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_l   "Left char 10"
      (start 1500) (end 1500) (filter .2) (fade $camera.fade)}
)
]

