   {venue switch_cam Cam_char_360 "Right char 1"
      (start 500) (end 400) (scale -.02)  (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_360 "Right char 2"
      (start 300) (end 300) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_360 "Right char 3"
      (start 600) (end 600) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_circle_wide "Right char 4"
      (start 350) (end 450) (scale .02) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_circle_wide "Right char 5"
      (start 450) (end 350)(scale -.02)(filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_circle_wide "Right char 6"
      (start 450) (end 450)            (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_r   "Right char 7"
      (start 480) (end 1560) (scale .15)  (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_r   "Right char 8"
      (start 1560) (end 480) (scale -.15) (filter .2) (fade $camera.fade)}

   {venue switch_cam Cam_char_r   "Right char 9"
