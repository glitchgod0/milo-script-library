{new HamPanel
   endgame_popup_panel
   (file "endgame_popup.milo")
   (show_hand_help FALSE)
   (header none)
   (item none)
   (item_description none)
   (enter
      {header.lbl set text_token [header]}
      {item.lbl set text_token [item]}
      {item_description.lbl set text_token [item_description]}
   )
   (exit)
   (set_popup_text
      ($header $item $item_description)
      {set [header] $header}
      {set [item] $item}
      {set [item_description] $item_description}
   )
   (SELECT_MSG
      {switch $component
         (ok.btn {ui go_back_screen perform_endgame_popup_between_screen})
         kDataUnhandled
      }
   )
}