{new HamPanel
   endgame_photos_panel
   (file "endgame_photos.milo")
   (original_leaderboards_mode 0)
   (helpbar_confirm_label none)
   (show_back_button TRUE)
   (enter
      {photo_display clear (textures)}
      {foreach_int $i 0 {gesture_mgr num_snapshots}
         {photo_display append (textures) {gesture_mgr snapshot_tex $i}}
      }
      {if {exists profile_mgr}
         {do
            ($trigger
               {find_obj
                  {$this loaded_dir}
                  photo_display
                  {if_else {profile_mgr get_disable_photos}
                     disable.trig
                     enable.trig
                  }
               }
            )
            {if $trigger
               {$trigger trigger}
            }
         }
      }
      {$this show_batch}
   )
   (SELECT_MSG
      {switch $component
         (back.btn {ui go_back_screen {{ui current_screen} get back}})
      }
   )
   (show_batch
      {do
         ($anim)
         {switch {game_panel get_type}
            (linear
               {switch {gesture_mgr num_snapshots}
                  (5
                     {set $anim
                        {find_obj {$this loaded_dir} photo_display performance_end_game_1.anim}
                     }
                  )
                  (9
                     {set $anim
                        {find_obj {$this loaded_dir} photo_display performance_end_game_2.anim}
                     }
                  )
                  (13
                     {set $anim
                        {find_obj {$this loaded_dir} photo_display performance_end_game_3.anim}
                     }
                  )
                  (17
                     {set $anim
                        {find_obj {$this loaded_dir} photo_display performance_end_game_4.anim}
                     }
                  )
               }
            )
            (multiplayer
               {set $anim
                  {find_obj {$this loaded_dir} photo_display multiplayer_end_game.anim}
               }
            )
         }
         {if $anim
            {$anim animate (loop 0 {$anim end_frame})}
         }
      }
   )
}
{new HamScreen
   endgame_photos_screen
   (panels meta background_panel endgame_photos_panel)
   (focus endgame_photos_panel)
   (back {endgame_photos_screen get back_screen})
   (back_screen none)
}