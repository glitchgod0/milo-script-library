{new UIPanel
   autosave_warning_panel
   (file "autosave_warning.milo")
   (enter {{$this loaded_dir} set in_transition TRUE})
   (poll
      {if {! {{$this loaded_dir} get in_transition}}
         {ui goto_screen splash_screen}
      }
   )
}
{new UIScreen autosave_warning_screen (panels autosave_warning_panel)}