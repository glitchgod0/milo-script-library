{new BandStorePanel
   store_panel
   (file "store.milo")
   (saved_offer_sym '')
   (offer_status_listener 0)
   (commerce_session_status_listener 0)
   (load {set [saved_offer_sym] ''})
   (enter
      {$this show_loading_thinger FALSE}
      #ifdef HX_PS3
      {ps_logo.mesh set_showing TRUE}
      {microsoft.lbl set_showing FALSE}
      #else
      {ps_logo.mesh set_showing FALSE}
      {microsoft.lbl set_showing TRUE}
      #endif
   )
   (show_loading_thinger
      ($b_show)
      {wait.grp set_showing $b_show}
   )
   (on_init_commerce_success
      {if {$this is_up}
         {if [commerce_session_status_listener]
            {[commerce_session_status_listener] on_init_commerce_success}
         }
      }
   )
   (offers_populated
      {$this update_loading_status}
      {if [offer_status_listener]
         {[offer_status_listener] on_offers_populated}
      }
   )
   (enum_start {$this show_loading_thinger TRUE})
   (enum_finished
      {if {$this is_up}
         {do
            {$this show_loading_thinger FALSE}
            {if [offer_status_listener]
               {[offer_status_listener] on_enum_finished}
            }
         }
      }
   )
   (enum_error
      {$this show_loading_thinger FALSE}
      {if_else {== {ui current_screen} prep_offer_for_store_screen}
         {store_info_unavailable_screen set
            return_screen
            {store_offer_fetch_panel get return_screen}
         }
         {store_info_unavailable_screen set return_screen store_menu_screen}
      }
      {ui goto_screen store_info_unavailable_screen}
   )
   (update_loading_status
      {$this show_loading_thinger {$this request_in_progress}}
   )
   (update_crumb_text {crumb.lbl set_store_crumb_text})
}
{new StoreMainPanel
   store_banner_panel
   (file "../store/store_main.milo")
   (display_rate 4.0)
   (crossfade_duration 1.0)
   (set_album_visibility
      ($bVisible)
      {album_art.grp set_showing $bVisible}
   )
}
{new StoreMenuPanel
   store_menu_panel
   (file "store_menu.milo")
   (menu_list "filter.lst")
   (focus filter.lst)
   (back_screen splash_screen)
   (enter
      {store_panel add_sink $this}
      {store_panel set offer_status_listener $this}
      {store_panel set commerce_session_status_listener $this}
      {store_panel update_crumb_text}
      {store_panel update_loading_status}
      {cond
         #ifdef HX_PS3
         ({!
               {trial_mgr get_full_offer_enumerate_successful}}
            {$this set_focus ""}
            {$this set_loading_visual_state unavailable}
         )
         #endif
         ({store_panel has_created_commerce_session}
            {$this on_init_commerce_success}
         )
         ({store_panel is_creating_commerce_session}
            {$this set_focus ""}
            {$this set_loading_visual_state loading}
         )
         ({store_panel has_aborted_commerce_session}
            {notify
               "StorePanel Commerce Session in an illegal state (kSessionAborted).\n"
            }
         )
         {fail
            "StorePanel Commerce Session in an illegal state (likely kSessionNone).\n"
         }
      }
   )
   (exit
      {store_panel set commerce_session_status_listener 0}
      {store_panel set offer_status_listener 0}
      {store_panel remove_sink $this}
   )
   (set_loading_visual_state
      ($new_state)
      {loading_state.ep set load_state $new_state}
      {if_else {== $new_state loaded}
         {store_banner_panel set_album_visibility TRUE}
         {store_banner_panel set_album_visibility FALSE}
      }
   )
   (new_provider
      {store_panel update_crumb_text}
      {store_panel update_loading_status}
      {$this set_focus filter.lst}
      {if_else {> {filter.lst num_data} 10}
         {scroller.grp set_showing TRUE}
         {scroller.grp set_showing FALSE}
      }
   )
   (on_init_commerce_success
      {$this request_index_if_needed}
      {if_else {$this has_menus}
         {do
            {$this set_loading_visual_state loaded}
            {$this refresh_current_menu}
         }
         {do
            {$this set_loading_visual_state loading}
         }
      }
   )
   (on_top_menu_populated {$this set_loading_visual_state loaded})
   (on_offers_populated
      #ifdef HX_PS3

      #else
      {$this process_offers}
      #endif
   )
   (on_enum_finished
      #ifdef HX_PS3
      {$this process_offers}
      #else

      #endif
   )
   (process_offers
      {if_else {>= {store_panel num_offers} 1}
         {ui goto_screen store_browser_screen}
         {$this refresh_current_menu}
      }
   )
   (back {ui goto_screen [back_screen]})
   (BUTTON_DOWN_MSG
      {cond
         ({== $action kAction_Cancel}
            {do
               ($ret TRUE)
               {if_else {&& {store_panel request_in_progress} {$this has_menus}}
                  {do
                     {store_panel cancel_current_request}
                     {$this set_focus filter.lst}
                  }
                  {set $ret {$this back}}
               }
               {store_panel update_loading_status}
               $ret
            }
         )
         ({== $action kAction_Start}
            {options_screen set back_screen store_menu_screen}
            {ui goto_screen options_screen}
         )
         ({== $action kAction_PageDown}
            {if {$this has_menus}
               {$this mark_current_highlight_idx}
            }
            {transition_to inbox_screen}
            {{background_panel find arrow_rt.flow} activate}
         )
         ({== $action kAction_PageUp}
            {if {$this has_menus}
               {$this mark_current_highlight_idx}
            }
            {transition_to song_select_screen}
            {{background_panel find arrow_lt.flow} activate}
         )
         {if_else {store_panel user_can_do_input}
            {switch $action
               (kAction_Option
                  {if_else {trial_mgr get_trial_mode}
                     {if {! {upsell_panel showing}}
                        {do
                           {{ui current_screen} show_upsell}
                           {{upsell_panel find enter.flow} activate}
                        }
                     }
                     {do
                        ($path {store_banner_panel marquee_path})
                        {if $path
                           {do
                              {store_info_enum_panel set_return_target store_menu_screen}
                              {store_info_enum_panel request_enum_and_show_single_offer
                                 marquee
                                 FALSE
                                 $path
                                 FALSE
                              }
                           }
                        }
                     }
                  }
               )
               kDataUnhandled
            }
            kDataUnhandled
         }
      }
   )
   (SELECT_MSG
      {if {&& {store_panel user_can_do_input} {$this has_menus}}
         {if_else {trial_mgr get_trial_mode}
            {do
               {{ui current_screen} show_upsell}
               {{upsell_panel find enter.flow} activate}
            }
            {do
               ($provider {$this get_menu_provider})
               ($ix {$component selected_pos})
               {$this mark_current_highlight_idx}
               {$this set_focus ""}
               {store_panel set_source
                  {symbol {sprintf "menu_%s" {$provider get_string $ix}}}
                  TRUE
               }
               {store_panel request {$provider get_string $ix} TRUE}
               {store_panel update_loading_status}
            }
         }
      }
   )
}
{new UIScreen
   store_menu_screen
   (panels
      background_panel
      shell_music_panel
      menu_cache_panel
      store_banner_panel
      store_menu_panel
      upsell_panel
      store_panel
   )
   SCREEN_CONTENT_REFRESH
   (focus store_menu_panel)
   (header_index 2)
   (which_store_screen store_menu_screen)
   (enter
      {background_panel jump_to_header [header_index]}
      {background_panel set_offline_mode_headers FALSE}
      {$this set which_store_screen $this}
      {update_participating_user_presence CONTEXT_PRESENCE_RP_BROWSING_STORE}
   )
   (show_upsell
      {upsell_panel set_options
         upsell_title_store
         upsell_text_store
         offer_trial_upgrade
      }
      {upsell_panel set_showing TRUE}
      {$this set_focus_panel upsell_panel}
   )
   (hide_upsell
      {upsell_panel set_showing FALSE}
      {$this set_focus_panel store_menu_panel}
   )
}
{new HeldButtonPanel
   store_browser_panel
   (file "store_browser.milo")
   (focus song.lst)
   (joypad
      (hold_ms 500)
      (repeat_ms 80)
   )
   (held_buttons)
   (saved_selected_pos -1)
   (saved_first_showing -1)
   (enter
      {store_panel set offer_status_listener $this}
      {store_panel set_source_to_backup}
      {store_panel update_crumb_text}
      {$this refresh_list}
      {if_else {== {store_panel num_offers} 0}
         {ui goto_screen store_menu_screen}
         {do
            {if {!= [saved_selected_pos] -1}
               {song.lst set_selected [saved_selected_pos] [saved_first_showing]}
            }
            {store_panel set saved_offer {song.lst selected_sym}}
            {$this refresh}
            {shortcuts.grp set_showing FALSE}
            {$this set_focus song.lst}
         }
      }
      {TransAnim.tnm set_frame 0}
   )
   (exit
      {{store_panel offer_provider} clear_list}
      {store_panel set offer_status_listener 0}
   )
   (refresh_list
      {do
         ($provider {store_panel offer_provider})
         ($sort_name {store_panel sort_name})
         {if_else {== $sort_name ""}
            {$provider build_list_no_grouping}
            {$provider build_list
               {find $syscfg store groupings $sort_name}
            }
         }
         {if_else {{store_panel offer_provider} has_shortcuts}
            {store.ihp set_action_token kAction_Option shortcut_header}
            {store.ihp clear_action_token kAction_Option}
         }
         {song.lst set_provider $provider}
         {song.lst set_selected 0}
      }
      {store_panel update_crumb_text}
   )
   (highlight_bottom
      {if {> {song.lst num_data} 0}
         {song.lst set_selected {- {song.lst num_data} 1}}
      }
   )
   (TRANSITION_COMPLETE_MSG {$this reset})
   (reset
      {set [saved_selected_pos] -1}
      {set [saved_first_showing] -1}
   )
   (on_button_held ($user $raw_button $action $pad_num))
   (show_shortcuts
      {if {{store_panel offer_provider} has_shortcuts}
         {shortcuts.grp set_showing TRUE}
         {$this set_focus shortcuts.lst}
         {store_panel set_shortcut_data
            {{store_panel offer_provider} get_shortcut_array}
         }
         {store_panel apply_shortcut_provider shortcuts.lst}
         {shortcuts.lst set_selected
            {{store_panel offer_provider} pos_to_shortcut {song.lst selected_pos}}
         }
      }
   )
   (turn_off_preview
      {store_preview_mgr clear_current_preview}
      {shell_music_panel set_mute_state FALSE}
   )
   (BUTTON_DOWN_MSG
      {if_else {{store_browser_panel find shortcuts.grp} showing}
         {cond
            ({== $action kAction_Cancel}
               {do
                  {store_browser_panel set_focus {store_browser_panel find song.lst}}
                  {{store_browser_panel find shortcuts.grp} set_showing FALSE}
               }
            )
            kDataUnhandled
         }
         {cond
            ({'||' {! {store_panel user_can_do_input}} {song.lst is_scrolling}} TRUE)
            ({== $action kAction_Cancel}
               {do
                  {$this turn_off_preview}
               }
               kDataUnhandled
            )
            ({== $action kAction_PageDown}
               {do
                  {$this turn_off_preview}
                  {transition_to inbox_screen}
                  {{background_panel find arrow_rt.flow} activate}
               }
            )
            ({== $action kAction_PageUp}
               {do
                  {$this turn_off_preview}
                  {transition_to song_select_screen}
                  {{background_panel find arrow_lt.flow} activate}
               }
            )
            ({== $action kAction_Start}
               {do
                  {$this turn_off_preview}
                  {options_screen set back_screen store_menu_screen}
                  {ui goto_screen options_screen}
               }
            )
            ({== $action kAction_Option}
               {do
                  {if {== {$this focus_name} "song.lst"}
                     {$this show_shortcuts}
                  }
               }
            )
            kDataUnhandled
         }
      }
   )
   (SCROLL_MSG
      {if {== $component song.lst}
         {store_panel set saved_offer {song.lst selected_sym}}
         {$this refresh}
      }
   )
   (refresh
      {if_else {$this selected_song}
         {do
            {store_details_panel set_showing TRUE}
            {store_details_panel refresh {$this selected_song}}
            {if_else
               {&&
                  {{$this selected_song} has_data preview_audio}
                  {is_allowed_to_preview_offer {$this selected_song}}
               }
               {do
                  {store_preview_mgr play_preview_file_if_cached
                     {sprintf
                        "%s%s"
                        {store_panel get_request_prefix}
                        {{$this selected_song} get_data (preview_audio)}
                     }
                  }
                  {if_else
                     {store_preview_mgr is_playing_file
                        {sprintf
                           "%s%s"
                           {store_panel get_request_prefix}
                           {{$this selected_song} get_data (preview_audio)}
                        }
                     }
                     {shell_music_panel set_mute_state TRUE}
                     {shell_music_panel set_mute_state FALSE}
                  }
               }
               {do
                  {$this turn_off_preview}
               }
            }
         }
         {store_details_panel set_showing FALSE}
      }
   )
   (SELECT_MSG
      {if_else {{store_browser_panel find shortcuts.grp} showing}
         {do
            {song.lst set_selected
               {{store_panel offer_provider} shortcut_to_pos {shortcuts.lst selected_sym}}
            }
            {$this turn_off_preview}
            {shortcuts.grp set_showing FALSE}
            {$this set_focus song.lst}
            {$this refresh}
         }
         {if_else {{store_panel offer_provider} is_chunk {song.lst selected_pos}}
            {do
               {$this set_focus ""}
               {if_else {== {song.lst selected_pos} 0}
                  {store_panel request_prev_chunk}
                  {store_panel request_next_chunk}
               }
            }
            {do
               {store_info_panel set reset TRUE}
               {store_panel set saved_offer {song.lst selected_sym}}
               {set [saved_selected_pos] {song.lst selected_pos}}
               {set [saved_first_showing] {song.lst first_showing}}
               {store_info_enum_panel set_return_target store_browser_screen}
               {store_info_enum_panel enum_and_show_offer {$this selected_song}}
            }
         }
      }
   )
   (selected_song
      {{store_panel offer_provider} find_offer {song.lst selected_sym}}
   )
   (on_offers_populated
      #ifdef HX_PS3

      #else
      {$this refresh_list}
      {if {store_panel should_start_browser_at_bottom}
         {$this highlight_bottom}
      }
      {$this set_focus song.lst}
      #endif
   )
   (on_enum_finished
      #ifdef HX_PS3
      {$this refresh_list}
      {if {store_panel should_start_browser_at_bottom}
         {$this highlight_bottom}
      }
      {$this set_focus song.lst}
      #else

      #endif
   )
}
{new HeldButtonPanel
   store_details_panel
   (file "store_details.milo")
   (cur_preview_art "")
   (joypad
      (hold_ms 500)
      (repeat_ms 80)
   )
   (finish_load {set [cur_preview_art] ""})
   (exit {store_panel cancel_art})
   (art_loaded {album.mat set diffuse_tex {store_panel album_tex}})
   (refresh
      ($offer)
      {artist.lbl set_offer_artist $offer}
      {album.lbl set_offer_album $offer}
      {if_else {$offer has_data genre}
         {genre.lbl set text_token {$offer get_data (genre)}}
         {genre.lbl set text_token ""}
      }
      {if_else {&& {$offer has_data year_released} {!= {$offer offer_type} pack}}
         {year.lbl set_int {$offer get_data (year_released)}}
         {year.lbl set text_token ""}
      }
      {if_else {== {$offer offer_type} song}
         {do
            {foreach $part (guitar drum vocals bass keys)
               {do
                  ($rank {$offer get_data (rank $part)})
                  ($tier {song_mgr rank_tier $rank $part})
                  ($idd {sprintf "diff_%s" $part})
                  {$this set_inst_difficulty $idd $tier TRUE}
               }
            }
         }
         {$this set_inst_difficulty "" -1 FALSE}
      }
      {if_else
         {&&
            {'||' {== {$offer offer_type} pack} {== {$offer offer_type} album}}
            {! {$offer is_completely_unavailable}}
         }
         {do
            ($provider {{store_panel offer_provider} get_pack_provider})
            {$provider set_pack $offer}
            {pack_songs.lst set_provider $provider}
            {pack_songs.lst set_selected 0}
            {pack_songs.lst auto_scroll}
            {pack_songs.lst set_showing TRUE}
         }
         {pack_songs.lst set_showing FALSE}
      }
      {if_else {&& {$offer has_data preview_art} {is_allowed_to_preview_offer $offer}}
         {unless {== [cur_preview_art] {$offer get_data (preview_art)}}
            {set [cur_preview_art] {$offer get_data (preview_art)}}
            {album.mat set diffuse_tex cover_art_none.tex}
            {store_panel load_art [cur_preview_art] $this}
         }
         {do
            {set [cur_preview_art] ''}
            {album.mat set diffuse_tex cover_art_none.tex}
         }
      }
   )
}
{new UIScreen
   store_browser_screen
   (panels
      background_panel
      shell_music_panel
      menu_cache_panel
      store_browser_panel
      store_panel
      store_details_panel
   )
   SCREEN_CONTENT_REFRESH
   (header_index 2)
   (focus store_browser_panel)
   (back_screen store_menu_screen)
   (back {store_browser_screen get back_screen})
   (enter
      {$this set_focus_panel store_browser_panel}
      {store_details_panel set_showing {int {store_browser_panel selected_song}}}
      {store_info_screen set back_screen $this}
      {store_menu_screen set which_store_screen $this}
   )
}