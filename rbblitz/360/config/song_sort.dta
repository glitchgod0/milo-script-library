(groupings
   (sort_artist
      (song_group
         ($song)
         {toupper {$song artist}}
      )
      (shortcut_group
         ($song)
         {$song first_char {$song artist} TRUE}
      )
      (localize_heading FALSE)
   )
   (sort_source
      (song_group
         ($song)
         {$song source}
      )
      (localize_heading TRUE)
   )
   (sort_song
      (song_group
         ($song)
         {$song first_char {$song title} TRUE}
      )
      (localize_heading FALSE)
   )
   (sort_stars
      (song_group
         ($song)
         {save_data get_star_rating_token {save_data get_star_rating {$song id}}}
      )
      (localize_heading TRUE)
   )
   (sort_diff
      (song_group
         ($song)
         {song_mgr rank_tier_token
            {song_mgr rank_tier {$song rank_for_part band} band}
         }
      )
      (localize_heading TRUE)
   )
   (sort_decade
      (song_group
         ($song)
         {sprintf "%i" {$song year_released}}
      )
      (localize_heading FALSE)
   )
   (sort_genre
      (song_group
         ($song)
         {$song genre_token}
      )
      (localize_heading TRUE)
   )
)
(order
   sort_artist
   sort_song
   sort_diff
   sort_stars
   sort_source
   sort_genre
   sort_decade
)
(star_tokens
   ((star star_sort1))
   ((star star_sort2))
   ((star star_sort3))
   ((star star_sort4))
   ((star star_sort5))
   ((star star_sort6))
   ((star star_sort7))
)