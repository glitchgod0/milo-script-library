   (time
      (lines 1)
      (showing FALSE)
      (color 0 0 0 0.7)
   )
   (delta_time
      (lines 1)
      (showing FALSE)
      (color 0 0 0 0.7)
   )
   (cheats
      (lines 1)
      (showing FALSE)
      (color 0 0 0 0.7)
   )
   (light_preset
      (lines 4)
      (showing FALSE)
      (color 0 0 0 0.5)
   )
   (camera
      (lines 2)
      (showing FALSE)
      (color 0 0 0 0.5)
   )
   (char_status
      (lines 4)
      (showing FALSE)
   )
   (eye_status
      (lines 4)
      (showing FALSE)
   )
   (score
      (lines 4)
      (showing FALSE)
   )
   (score_debug
      (lines 1)
      (showing FALSE)
   )
   (lights
      (lines 1)
      (showing FALSE)
   )
   (camtest
      (lines 1)
      (showing FALSE)
   )
   (latency
      (lines 2)
      (showing FALSE)
   )
   (network
      (lines 10)
      (showing FALSE)
   )
   (track_state
      (lines 1)
      (showing FALSE)
   )
   (prop_states
      (lines 5)
      (showing FALSE)
   )
)
(timer_script
   {set_timer_ms hud_track
      {'+' {timer_ms hud_track_poll} {timer_ms hud_track_draw}}
   }
   {set_timer_ms world {'+' {timer_ms world_poll} {timer_ms world_draw}}}
   {set_timer_ms ui_poll
      {-
         {timer_ms ui_poll_raw}
         {'+' {timer_ms spline_deform_poll} {timer_ms beat_anims}}
      }
   }
   {show_timer draw_track $subtime_draw}
   {show_timer draw_widgets $subtime_draw}
   {show_timer draw_gem_tails $subtime_draw}
   {show_timer cull_plots $subtime_draw}
   {show_timer draw_plots_regular $subtime_draw}
   {show_timer draw_plots_skinned $subtime_draw}
   {show_timer draw_terrain $subtime_draw}
   {show_timer draw_skybox $subtime_draw}
   {show_timer draw_world_misc $subtime_draw}
   {show_timer draw_other $subtime_draw}
   {show_timer draw_light_approx $subtime_draw}
   {if $subtime_draw
      {do
         ($draw_other_time {timer_ms draw})
         {-= $draw_other_time {timer_ms draw_track}}
         {-= $draw_other_time {timer_ms draw_widgets}}
         {-= $draw_other_time {timer_ms draw_gem_tails}}
         {-= $draw_other_time {timer_ms cull_plots}}
         {-= $draw_other_time {timer_ms draw_plots_regular}}
         {-= $draw_other_time {timer_ms draw_plots_skinned}}
         {-= $draw_other_time {timer_ms draw_terrain}}
         {-= $draw_other_time {timer_ms draw_skybox}}
         {-= $draw_other_time {timer_ms draw_world_misc}}
         {set_timer_ms draw_other $draw_other_time}
      }
   }
   {do
      ($app_misc_time 0)
      {'+=' $app_misc_time {timer_ms system_poll}}
      {'+=' $app_misc_time {timer_ms ui_poll}}
      {'+=' $app_misc_time {timer_ms net_poll}}
      {'+=' $app_misc_time {timer_ms rock_central_poll}}
      {'+=' $app_misc_time {timer_ms flow_poll}}
      {'+=' $app_misc_time {timer_ms spline_deform_poll}}
      {set_timer_ms app_misc $app_misc_time}
   }
   {show_timer system_poll $subtime_app_misc}
   {show_timer ui_poll $subtime_app_misc}
   {show_timer net_poll $subtime_app_misc}
   {show_timer rock_central_poll $subtime_app_misc}
   {show_timer flow_poll $subtime_app_misc}
   {show_timer spline_deform_poll $subtime_app_misc}
   {do
      ($cpu_other_time {timer_ms cpu})
      {-= $cpu_other_time {timer_ms draw}}
      {-= $cpu_other_time {timer_ms anim}}
      {-= $cpu_other_time {timer_ms synth_poll}}
      {-= $cpu_other_time {timer_ms app_misc}}
      {set_timer_ms cpu_other $cpu_other_time}
   }
)
(text_superscript_scale 0.42)
(text_guitar_z_offset 1.0)
(text_guitar_scale 0.7)
#ifdef HX_PS3
(compression_buffer 2097152)
#endif