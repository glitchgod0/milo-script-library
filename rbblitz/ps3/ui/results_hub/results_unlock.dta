{new UIPanel
   results_unlock_panel
   (file "results_unlock.milo")
   (powerup_index 0)
   (BUTTON_DOWN_MSG
      {switch $action
         (kAction_Confirm
            {if_else
               {>=
                  [powerup_index]
                  {{{game_panel get_game} get_results} get_num_unlocked_powerups}
               }
               {transition_to career_results_screen}
               {$this update_powerup TRUE}
            }
            {shell_sfx play_select}
         )
         (kAction_PageDown)
         (kAction_Cancel
            {if_else {<= [powerup_index] 1}
               {transition_to song_results_screen}
               {do
                  {set [powerup_index] {- [powerup_index] 2}}
                  {$this update_powerup FALSE}
               }
            }
            {shell_sfx play_back}
         )
         (kAction_PageUp)
      }
   )
   (enter
      {set [powerup_index] 0}
      {$this update_powerup TRUE}
   )
   (update_powerup
      ($going_forward)
      {do
         ($powerup
            {{{game_panel get_game} get_results} get_unlocked_powerup [powerup_index]}
         )
         {unlock.ep set going_forward $going_forward}
         {unlock.ep set powerup_desc {sprintf "%s_desc" $powerup}}
         {unlock.ep set powerup $powerup}
      }
      {set [powerup_index] {'+' 1 [powerup_index]}}
   )
}
{new UIScreen
   results_unlock_screen
   (panels
      background_night_panel
      results_shared_panel
      results_cache_panel
      game_panel
      game_fade_out_panel
      results_unlock_panel
   )
   (header_index 12)
   (focus results_unlock_panel)
}