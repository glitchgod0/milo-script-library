(controller
   #include beatmatch_controller.dta)
(midi_parsers
   #include midi_parsers.dta)
(callbacks
   (reset
      {handle
         ($world start)})
   (eighth_note
      {handle
         ($world halfbeat)})
   (beat
      {handle
         ($world beat)}
      {handle
         (game beat)})
   (sixteenth_note
      {if_else
         $world
         {handle
            ($world quarterbeat)}
         {handle
            (game quarterbeat)}})
   (downbeat
      {if_else
         $world
         {handle
            ($world downbeat)}
         {handle
            (game downbeat)}}))
(watcher
   (can_switch_tracks FALSE)
   (slop 100)
   (cheat_error 0)
   (autoplay_accuracy
      (100 100 100 100))
   (pitch_bend_range 1)
   (ms_to_full_pitch_bend 1000))
(parser
   (drum_style_instruments
      (drum))
   (vocal_style_instruments
      (vocals))
   (num_slots 5)
   (phrase_slot 7)
   (player_slot 9))
(audio
   (default_style unmute)
   (mute_volume -96.0)
   (multiplayer_scale 0.33)
   (multiplayer_attenuation 5.25)
   (multiplayer_common_attenuation 3.0)
   (background_attenuation 3.0)
   (cue_attenuation 0.0)
   (remote_attenuation 2.5)
