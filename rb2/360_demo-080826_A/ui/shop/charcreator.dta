{new
   BandScreen
   charcreator_cancel_screen
   (panels dialog_panel)
   (focus dialog_panel)
   (enter
      {dialog_panel
         set_yesno
         {localize shell_charcreator_cancel}
         no.btn})
   (SELECT_MSG
      {switch
         $component
         (yes.btn
            {ui
               pop_screen
               {gamemode get matchmaking_screen}})
         (no.btn
            {ui pop_screen})})}
{new
