{new
   UIPanel
   eyecolor_panel
   (file
      "color_select.milo")
   (focus color.lst)
   (undo_eye_index -1)
   (enter
      {title.lbl set text_token shell_eye_color}
      {do
         ($char
            {charcreator_panel get_char})
         {color.lst
            set_colors
            {$char get eye_color_palette}}
         {color.lst
            set_selected
            {$char get eye_color_index}}
         {set
            [undo_eye_index]
            {$char get eye_color_index}}})
   (SCROLL_MSG
      {{charcreator_panel get_tour_char}
         set_eye_color
         {charcreator_panel get_char}
         {color.lst selected_pos}})
   (SELECT_MSG
      {charcreator_face_panel confirm_outfit}
      {ui pop_screen})
   (BUTTON_DOWN_MSG
      {if
         {== $action kAction_ViewModify}
         {synth play button_toggle}
         {practice_space_panel select_next_cam}}
      {if
         {== $action kAction_Cancel}
         {{charcreator_panel get_tour_char}
            set_eye_color
            {charcreator_panel get_char}
            [undo_eye_index]}
         {ui pop_screen}}
      kDataUnhandled)}
{new
   BandScreen
   eyecolor_screen
   (panels eyecolor_panel)
   (focus eyecolor_panel)}
{new
   OutfitPanel
   charcreator_face_panel
   (file
      "charcreator_face.milo")
   (focus heads.btn)
   (reset_focus TRUE)
   (show_outfit_text TRUE)
   (select_cam FALSE)
   (outfit_char charcreator_panel)
   (in_shop FALSE)
   (enter
      {do
         ($tour_char
            {charcreator_panel get_tour_char})
         ($char
            {charcreator_panel get_char})
         {eye_color.lst
            set_colors
            {$char get eye_color_palette}}
         {eye_color.lst
            set_selected
            {$char get eye_color_index}}
         {if
            {==
               {$tour_char get_gender}
               female}
            {do
               {$this disable facehair.btn}
               {facehair.btn set_showing FALSE}
               {facehair.lst set_showing FALSE}}}})
   (SELECT_MSG
      {unless
         {==
            {$this focus_category}
            ''}
         {$this store_outfit}}
      {switch
         $component
         (eye_color.btn
            {eye_color.lst store}
            {$this set_focus eye_color.lst})
         (done.btn
            {ui goto_screen charcreator_maker_screen})})
   (TRANSITION_COMPLETE_MSG
      {practice_space_panel select_cam creator_heads})
   (SCROLL_MSG
      {do
         ($tour_char
            {charcreator_panel get_tour_char})
         ($char
            {charcreator_panel get_char})
         {switch
            $component
            (eye_color.lst
               {$tour_char
                  set_eye_color
                  $char
                  {eye_color.lst selected_pos}})}})
   (BUTTON_DOWN_MSG
      {if
         {== $action kAction_ViewModify}
         {synth play button_toggle}
         {practice_space_panel select_next_cam}}
      {do
         ($category
            {$this focus_category})
         ($selected
            {$this focus_outfit})
         {if_else
            {!= $selected ''}
            {switch
               $action
               (kAction_Confirm
                  {cond
                     ({== $category heads}
                        {$this confirm_outfit})
                     ({'||'
                           {== $category hair}
                           {== $category facehair}}
                        {if_else
                           {$this find_outfit_config}
                           {do
                              {color_select_panel set color_idx 0}
                              {color_select_panel set outfit_panel $this}
                              {ui push_screen color_select_screen}}
                           {$this confirm_outfit}})})
               (kAction_Cancel
                  {$this undo_outfit}
                  TRUE)
               kDataUnhandled}
            {switch
               {$this focus_name}
               (eye_color.lst
                  {switch
                     $action
                     (kAction_Confirm
                        {eye_color.lst confirm}
                        {$this set_focus eye_color.btn})
                     (kAction_Cancel
                        {eye_color.lst undo $player}
                        {$this set_focus eye_color.btn})
                     kDataUnhandled})
               kDataUnhandled}}})
   (on_color_select
      ($category $index $color)
      {if
         {&&
            {== $category hair}
            {== $index 0}}
         {do
            ($tour_char
               {charcreator_panel get_tour_char})
            ($char
               {charcreator_panel get_char})
            {$tour_char set_eyebrow_color $char $color}}})}
{new
   BandScreen
   charcreator_face_screen
   (panels meta charcreator_panel practice_space_panel charcreator_face_panel tour_lower3rd_panel)
   (helpbar
      ((cancel helpbar_back)
         (confirm helpbar_select)
         (view_modify shop_hb_change_camera)))
   (focus charcreator_face_panel)
   (back charcreator_maker_screen)}
{new
   OutfitPanel
   charcreator_body_panel
   (file
      "charcreator_body.milo")
   (focus weight.btn)
   (reset_focus TRUE)
   (show_outfit_text TRUE)
   (select_cam FALSE)
   (outfit_char charcreator_panel)
   (in_shop FALSE)
   (enter
      {do
         ($tour_char
            {charcreator_panel get_tour_char})
         ($char
            {charcreator_panel get_char})
         {weight.sld set_num_steps 5}
         {weight.sld
            set_frame
            {$char get weight}}
         {height.sld set_num_steps 5}
         {height.sld
            set_frame
            {$char get height}}
         {skin.lst
            set_colors
            {$char get skin_color_palette}}
         {skin.lst
            set_selected
            {$char get skin_color_index}}})
   (SELECT_MSG
      {unless
         {==
            {$this focus_category}
            ''}
         {$this store_outfit}}
      {switch
         $component
         (weight.btn
            {weight.sld store}
            {$this set_focus weight.sld})
         (height.btn
            {height.sld store}
            {$this set_focus height.sld})
         (skin.btn
            {skin.lst store}
            {$this set_focus skin.lst})
         (done.btn
            {ui goto_screen charcreator_maker_screen})})
   (SCROLL_MSG
      {do
         ($tour_char
            {charcreator_panel get_tour_char})
         ($char
            {charcreator_panel get_char})
         {switch
            $component
            (skin.lst
               {$tour_char
                  set_skin_color
                  $char
                  {skin.lst selected_pos}})
            (weight.sld
               {$tour_char
                  set_weight
                  $char
                  {weight.sld frame}}
               {$char deform FALSE})
            (height.sld
               {$tour_char
                  set_height
                  $char
                  {height.sld frame}}
               {$char deform FALSE})}})
   (TRANSITION_COMPLETE_MSG
      {practice_space_panel select_cam creator})
   (BUTTON_DOWN_MSG
      {if
         {== $action kAction_ViewModify}
         {synth play button_toggle}
         {practice_space_panel select_next_cam}}
      {do
         ($category
            {$this focus_category})
         ($selected
            {$this focus_outfit})
         {if_else
            {!= $selected ''}
            {switch
               $action
               (kAction_Confirm
                  {cond
                     (TRUE
                        {$this confirm_outfit})})
               (kAction_Cancel
                  {$this undo_outfit}
                  TRUE)
               kDataUnhandled}
            {switch
               {$this focus_name}
               (skin.lst
                  {switch
                     $action
                     (kAction_Confirm
                        {skin.lst confirm}
                        {$this set_focus skin.btn})
                     (kAction_Cancel
                        {skin.lst undo $player}
                        {$this set_focus skin.btn})
                     kDataUnhandled})
               (weight.sld
                  {switch
                     $action
                     (kAction_Confirm
                        {weight.sld confirm}
                        {$this set_focus weight.btn})
                     (kAction_Cancel
                        {weight.sld undo $player}
                        {$this set_focus weight.btn})
                     kDataUnhandled})
               (height.sld
                  {switch
                     $action
                     (kAction_Confirm
                        {height.sld confirm}
                        {$this set_focus height.btn})
                     (kAction_Cancel
                        {height.sld undo $player}
                        {$this set_focus height.btn})
                     kDataUnhandled})
               kDataUnhandled}}})}
