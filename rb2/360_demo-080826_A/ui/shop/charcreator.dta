{new
   CharCreatorPanel
   charcreator_panel
   (load
      {charcreator_maker_panel set reset_name_focus TRUE})
   (enter
      {practice_space_panel
         set_creator_user
         {$this input_user}}
      {practice_space_panel select_cam creator})
   (unload
      {practice_space_panel set_creator_user ''}
      {input_mgr clear_user})
   (on_owner_user_signed_out
      {ui
         reset_screen
         {gamemode get matchmaking_screen}})}
{new
   CharLoadingPanel
   charcreator_gender_panel
   (file
      "charcreator_gender.milo")
   (enter
      {do
         ($user
            {charcreator_panel input_user})
         {gamertag.lbl
            set_localized
            {$user get player_name}}
         {instrument.lbl
            set_localized
            {get_font_char_from_controller_type
               {$user get_controller_type}}}}
      {$this
         set_focus
         "male.btn"}
      {presence_mgr set_making_a_rocker})
   (FOCUS_MSG
      {if
         {'||'
            {== $new_focus male.btn}
            {== $new_focus female.btn}}
         {charcreator_panel
            request
            {basename
               {$new_focus name}}}})
   (SELECT_MSG
      {ui goto_screen charcreator_maker_screen})}
{new
