(songs_dta "D:/prj/band2/band2/run/songs_download/songs_archive.dta")
#define SONG_POOL_CATEGORIES (intro verse prechorus chorus solo bridge outro)
(process_single
   {set $results {array 0}}
   {set [working_track] "EVENTS"}
   {$this foreach_textevent $text
      $tick
      {if {has_substr $text "[section "}
         {search_replace $text "[section " "" $sectionName}
         {search_replace $sectionName "]" "" $sectionName}
         {set $categoryName "CatchAll"}
         {set $numFound 0}
         {foreach $cat (SONG_POOL_CATEGORIES)
            {if {has_substr $sectionName $cat}
               {set $categoryName $cat}
               {'+=' $numFound 1}
            }
         }
         {push_back
            $results
            {array
               ({file_get_base
                     [datamine_file]}
                  [datamine_genre]
                  $sectionName
                  $categoryName
                  {> $numFound 1}
               )
            }
         }
      }
   }
   $results
)
(tabulate_results
   {set $textFile
      {new File {sprint "d:/temp/datamine/datamine_practicesections.csv"}}
   }
   {$textFile printf "Song, Genre, SectionName, Category, HasConflict\n"}
   {$this foreach_datamine_result $result
      {foreach_int $i 0 {size $result}
         {set $curRes {elem $result $i}}
         {$textFile printf
            "%s, %s, %s, %s, %d\n"
            {elem $curRes 0}
            {elem $curRes 1}
            {elem $curRes 2}
            {elem $curRes 3}
            {elem $curRes 4}
         }
      }
   }
   {$textFile set_name ""}
)