{func anim_bad
   {arena switch_anim blast.matanim (range 0 0) (scale 2.0e-2) (blend 0)}
   {animate_to arena reactor_set.view 0 3840}
   {arena switch_anim nuke_toxic.partanim (range 500 500) (scale 1) (blend 0)}
   {arena delay_task
      3840
      {arena switch_anim toxic4.partanim (range 1 1) (scale 1) (blend 0)}
   }
}
{func anim_okay
   {arena switch_anim blast.matanim (range 0 0) (scale 2.0e-2) (blend 0)}
   {animate_to arena reactor_set.view 0 3840}
   {arena switch_anim nuke_toxic.partanim (range 500 500) (scale 1) (blend 0)}
   {arena delay_task
      3840
      {arena switch_anim toxic4.partanim (range 1 1) (scale 1) (blend 0)}
   }
}
{func anim_great
   {animate_to arena reactor_set.view 50 3840}
   {arena switch_anim toxic4.partanim (range 0 0) (scale 1) (blend 0)}
   {arena delay_task
      1200
      {arena switch_anim nuke_toxic.partanim (range 0 500) (scale 1) (blend 0)}
      {arena switch_anim_rt blast.matanim (range 0 9) (scale 2.0e-2) (blend 0)}
   }
}
{func hit_speakers
   ($speakers)
   {foreach $anim $speakers
      {arena switch_anim $anim (range {random_int 70 100} 0) (scale 1) (blend 0)}
   }
}
{func hit_speakers_lite
   ($speakers)
   {foreach $anim $speakers
      {arena switch_anim $anim (range {random_int 45 75} 0) (scale 1) (blend 0)}
   }
}
{func hit_guitar_amp
   ($slot)
   {hit_speakers
      {switch $slot
         (0 (cone1.tnm cone2.tnm))
         (1 (cone1.tnm cone2.tnm))
         (2 (cone1.tnm cone2.tnm))
         (3 (cone3.tnm cone4.tnm))
         (4 (cone3.tnm cone4.tnm))
      }
   }
   {switch $slot
      (0 {hit_speakers_lite (cone3.tnm cone4.tnm)})
      (1 {hit_speakers_lite (cone3.tnm cone4.tnm)})
      (2 {hit_speakers_lite (cone3.tnm cone4.tnm)})
      (3 {hit_speakers_lite (cone1.tnm cone2.tnm)})
      (4 {hit_speakers_lite (cone1.tnm cone2.tnm)})
   }
}
{arena add_handlers
   (init
      {bad.anim unhook_anim_parents}
      {lightOK.anim unhook_anim_parents}
      {lightGR.anim unhook_anim_parents}
      {verse.anim unhook_anim_parents}
      {full.anim unhook_anim_parents}
      {chorus.anim unhook_anim_parents}
      {solo.anim unhook_anim_parents}
      {venue_main.anim unhook_anim_parents}
      {reactor_set.view unhook_anim_parents}
      {reactor_set.view set_frame 0}
      {kutsom_stack.view unhook_anim_parents}
      {swr_rig.view unhook_anim_parents}
      {drum_kit.view unhook_anim_parents}
   )
   (finish_load {skysphere.mesh set_showing TRUE})
   (intro_start {anim_bad})
   (music_start {anim_okay})
   (terminate
      {delete set_lights_bad}
      {delete set_lights_okay_verse}
      {delete set_lights_okay_chorus}
      {delete set_lights_okay_solo}
      {delete set_lights_great_verse}
      {delete set_lights_great_chorus}
      {delete set_lights_great_solo}
      {delete anim_bad}
      {delete anim_okay}
      {delete anim_great}
   )
   (excitement_bad {anim_bad})
   (excitement_okay {anim_okay})
   (excitement_great {anim_great})
   (set_lights_bad {set_lights_bad})
   (set_lights_okay_verse {set_lights_okay_verse})
   (set_lights_okay_chorus {set_lights_okay_chorus})
   (set_lights_okay_solo {set_lights_okay_solo})
   (set_lights_great_verse {set_lights_great_verse})
   (set_lights_great_chorus {set_lights_great_chorus})
   (set_lights_great_solo {set_lights_great_solo})
   (game_won {anim_great})
   (game_lost {anim_bad})
   (finish_loading {"6 foot camera.cam" add_trans rim_light.lit})
   (kick_drum)
   (bass_hit
      {hit_speakers
         (swr_cone1.tnm swr_cone2.tnm swr_cone3.tnm swr_cone4.tnm)
      }
   )
   (hit_gem {hit_guitar_amp $slot})
}