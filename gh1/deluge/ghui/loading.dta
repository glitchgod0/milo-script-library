{func reset_ambient
   {'ui::ui.enm' set_frame 500}
}
{new_panel loading (enter {tip.lbl set_localized_text {tips random_tip}})}
{new_gh_screen loading
   (panels loading)
   (allow_back FALSE)
   (animate_transition FALSE)
   (enter
      {reset_ambient}
      {synth stop_all_sfx}
      {animate_forever_30fps $this loading.view}
      {{game get_bank_loader} reset}
      {ui goto_screen game}
   )
}