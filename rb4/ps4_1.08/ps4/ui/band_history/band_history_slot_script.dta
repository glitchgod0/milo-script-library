(define Interface 
   (description "Band History Slot Manager") 
   (props 
      (current_state int 
         (help "What is the current state of this slot?") 
         (set_on_instance TRUE) 
         (default 0) 
         (allowed_values 
            (
               (0 "inactive" "Not yet active") 
               (1 "next_up" "The next tour players are working towards") 
               (2 "starting" "Starting now") 
               (3 "in_progress" "Currently in progress") 
               (4 "ending" "Ending now") 
               (5 "completed" "Completed")))) 
      (slot_position symbol 
         (help "Where is this slot displayed in the set?") 
         (set_on_instance TRUE) 
         (default hometown) 
         (allowed_values 
            (
               (hometown "hometown" "As the starting Hometown slot in the set") 
               (top_row "top_row" "In the top row of the set") 
               (bottom_row "bottom_row" "In the bottom row of the set") 
               (finale "finale" "As the Finale slot in the set")))) 
      (slot_icon symbol 
         (help "Which icon should this slot display?") 
         (set_on_instance TRUE) 
         (default no_icon) 
         (allowed_values 
            (
               (no_icon "no_icon" "No icon") 
               (tour_city_amsterdam "tour_city_amsterdam" "Amsterdam") 
               (tour_city_austin "tour_city_austin" "Austin") 
