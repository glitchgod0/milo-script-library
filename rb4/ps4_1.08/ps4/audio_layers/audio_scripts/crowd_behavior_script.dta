(define Interface 
   (description "Crowd Behavior Bools") 
   (props 
      (Exalted bool 
         (default FALSE)) 
      (Normal bool 
         (default TRUE)) 
      (Hostile bool 
         (default FALSE)) 
      (Crowd_Laughs bool 
         (default FALSE) 
         (help "any crowd cam, mood specific audio/anim")) 
      (Crowd_Listens bool 
         (default FALSE) 
         (help "any crowd cam, mood specific audio/anim")) 
      (Player_Talking bool 
         (default FALSE) 
         (help "lead singer cam shots (or a shot from behind the lead singer overlooking the crowd")) 
      (Crowd_Cheers bool 
         (default FALSE) 
         (help "any crowd cam, mood specific audio/anim")) 
      (Crowd_Impatient bool 
         (default TRUE) 
         (help "any crowd cam, impatient anim, impatient audio")) 
      (Beat_Clapping bool 
         (default FALSE) 
         (help "any crowd cam, beat clap anim, beat clap audio")) 
      (Single_Call bool 
         (default FALSE) 
         (help "single char shot, call anim, one-off audio (mood/context specific audio/anim)")) 
      (Crowd_Disappointed bool 
         (default FALSE) 
         (help "any crowd shot, disappointed anim, disappointed audio")) 
      (Encore_Voting bool 
         (default FALSE) 
         (help "any crowd shot, 'one more song' anim, 'one mroe song' audio")) 
      (Intro_to_VocalScript bool 
         (default FALSE) 
         (help "toggles whether or not the player is just arriving at a vocal script")) 
      (Norm_Mellow_Override bool 
         (default FALSE) 
         (help "toggle this when an anim text event is present to override the anims")) 
      (Crowd_fullImpatient bool 
         (default FALSE) 
         (help "This event fires is toggled on when the crowd actually goes into an impaitent state")) 
      (First_CamChange bool 
         (default TRUE) 
         (help "This bool is used to tell the cam to change only on the first player_listens event")) 
      (Upto_Exalted bool 
         (default FALSE) 
         (help "This bool will let us know if we should do some extra fanfair since the player has gone up to Exalted")) 
      (slot0Request int) 
      (slot1Request int) 
      (slot2Request int) 
      (slot3Request int) 
      (rnd_float float) 
      (new_rnd bool 
         (default FALSE)) 
      (crd_num_target float 
         (help "This float is 0 to 1 and is an abstracted value of performance based on both the crowd meter and crowd mood")) 
      (request_rnd_float float)))
(enter 
   {set 
      [$this rnd_float ]
      {random_float 3 10 }})
(poll 
   {if 
      [$this new_rnd ]
      {set 
         [$this rnd_float ]
         {random_float 3 10 }}
      {set 
         [$this new_rnd ]FALSE }} 
   {set 
      [$this request_rnd_float ]
      {random_float 0 1 }})
