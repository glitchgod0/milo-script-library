(WorldDir
   (types
      (small1
         WORLD_OBJECT_BASE
         (sound
            (bank small1_bank)
            (crowd
               (intro -8.0 crowd_v2_0intro)
               (levels
                  (kExcitementBoot -10.5 crowd_v2_1danger)
                  (kExcitementBad -10.5 crowd_v2_2poor)
                  (kExcitementOkay -14.0 crowd_v2_3norm)
                  (kExcitementGreat -10.5 crowd_v2_4good)
                  (kExcitementPeak -8.0 crowd_v2_4good)
               )
               (encore_outro 0.0 crowd_v2_0intro)
               (encore_intro 0.0 crowd_v2_0intro)
            )
         )
         (camera_durations
            (
               (kExcitementBoot 3 6)
               (kExcitementBad 3 6)
               (kExcitementOkay 3 5)
               (kExcitementGreat 2 4)
               (kExcitementPeak 2 4)
            )
         )
         (intro_camera_distance near)
         (intro_camera_facing right)
      )
   )
)
(RndDir
   (types
      (bottle_throw
         (editor
            (start script (script {$this start}))
            (stop script (script {$this stop}))
         )
         (start
            {$this stop}
            {script_task
               (units kTaskSeconds)
               (delay {random_float 0 10})
               (name script_task1)
               (script
                  {$this set_showing TRUE}
                  {$this throw}
               )
            }
         )
         (throw
            {$this animate
               (units kTaskSeconds)
               (period {random_float 1.5 2.1})
               (range 0 63)
               (name anim_task)
            }
            {script_task
               (units kTaskSeconds)
               (delay {random_float 6 10})
               (script {$this throw2})
               (name script_task2)
            }
         )
         (throw2
            {$this animate
               (units kTaskSeconds)
               (period {random_float 1.5 2.1})
               (range 0 63)
               (name anim_task)
            }
            {script_task
               (units kTaskSeconds)
               (delay {random_float 6 10})
               (script {$this throw})
               (name script_task3)
            }
         )
         (stop
            {$this set_showing FALSE}
            {if {exists anim_task}
               {delete anim_task}
            }
            {if {exists script_task1}
               {delete script_task1}
            }
            {if {exists script_task2}
               {delete script_task2}
            }
            {if {exists script_task3}
               {delete script_task3}
            }
         )
      )
   )
)