         (range 1 50)
         (help "Number of rows/columns")
      )
      (grid_span
         int
         (range 1 50)
         (help "Number \\" 'across\\"' for a 'grid."')
      )
      (circular bool (help "Does the list scrolling wrap?"))
      (limit_circular_display_num_to_data_num
         bool
         (help "Allow multiple instances of same option to be displayed?")
      )
      (scroll_time
