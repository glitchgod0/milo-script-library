(loading_screens
   (noop "loading_blank.milo")
   (mirror_tutorial "loading_mirror2p_tutorial_keep.milo")
   (dancebattlevs_tutorial "loading_dancebattlevs_tutorial_keep.milo")
   (freestyle "loading_freestyle.milo")
   (workit "loading_freestyle_workit.milo")
)
(loading_screen_context
   (dancebattlevs_tutorial
      (weight
         {if_else {gamemode in_mode dance_battle}
            1
            0
         }
      )
   )
   (workit
      (weight
         {if_else {&& {hamprovider get is_in_campaign_mode} {campaign get work_it_active}}
            1
            0
         }
      )
   )
   (mirror_tutorial
      (weight
         {if_else
            {&&
               {hamprovider get is_in_campaign_mode}
               {meta_performer is_campaign_new}
               {! {campaign get work_it_active}}
            }
            1
            {if_else
               {&&
                  {! {gamemode in_mode dance_battle}}
                  {! {gamemode in_mode rhythm_battle}}
                  {!
                     {&& {hamprovider get is_in_campaign_mode} {campaign get work_it_active}}
                  }
                  {!
                     {&&
                        {hamprovider get is_in_campaign_mode}
                        {== {campaign get_campaign_state} kCampaignStateTanBattleComplete}
                     }
                  }
               }
               3
               0
            }
         }
      )
   )
   (freestyle
      (weight
         {if_else
            {&&
               {! {gamemode in_mode dance_battle}}
               {'||'
                  {! {hamprovider get is_in_campaign_mode}}
                  {&& {! {meta_performer is_campaign_new}} {! {campaign get work_it_active}}}
               }
            }
            17
            0
         }
      )
   )
)