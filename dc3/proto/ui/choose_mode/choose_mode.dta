{new ChooseModeProvider choose_mode_provider}
{new HamPanel
   choose_mode_panel
   (file "choose_mode.milo")
   (focus right_hand.hnl)
   (back_token screen_name_main)
   (has_overlay FALSE)
   (enter
      #ifdef DEMO
      {exit_party_mode}
      #else

      #endif
      {{cursor_panel find depth_buffer_left.grp} set_showing TRUE}
      {{cursor_panel find depth_buffer_right.grp} set_showing TRUE}
      {hamprovider set force_depth_buffer_skeletons_to_side -1}
      {cursor_panel reset_sided_colors}
      {cursor_panel reset_silhouettes}
      #ifdef DEMO
      {content_mgr refresh_synchronously}
      {shell_input exit_controller_mode}
      {meta music_start}
      #endif
      #ifdef DEMO
      {$this set back_token ""}
      {$this set tertiary_token options}
      #else

      #endif
      {hamprovider set ui_nav_mode shell}
      {gamemode set_mode init}
      {hamprovider set is_in_campaign_mode FALSE}
      {hamprovider set is_in_campaign_master_quest_mode FALSE}
      {$this refresh}
      #ifdef DEMO
      {description.grp set showing FALSE}
      {logo.grp set showing 1}
      {legal.lbl set showing 1}
      #else
      {description.lbl set text_token choose_mode_description_perform_it}
      {game_mode_icon set_mode perform}
      {game_mode_icon show}
      #endif
      {set [has_overlay] {! {ui has_overlay_panel}}}
      {voice_input_panel activate_voice_context choose_mode_screen}
   )
   (refresh
      {choose_mode_provider update_list {ui show_dev_menu}}
      {right_hand.hnl set_provider choose_mode_provider}
   )
   SELECT_RIGHT_LIST_TOKEN
   (poll
      SLIDE_RIGHT_LIST_ITEM
      {if {!= {ui has_overlay_panel} [has_overlay]}
         {if_else {ui has_overlay_panel}
            {do
               {set [has_overlay] TRUE}
               {$this set_showing FALSE}
               {right_hand.hnl disable_navigation}
            }
            {do
               {set [has_overlay] FALSE}
               {$this set_showing TRUE}
               {right_hand.hnl enable_navigation}
            }
         }
      }
   )
   (NAV_SELECT_MSG
      {switch $name
         (perform_legacy
            {gamemode set_mode perform_legacy}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (perform
            {gamemode set_mode perform}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (practice
            {gamemode set_mode practice}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (dance_battle
            {gamemode set_mode dance_battle}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         #ifdef DEMO
         (party_mode
            {do
               ($perform_on {partymode_mgr is_mode_included perform})
               ($bustamove_on {partymode_mgr is_mode_included bustamove})
               ($rhythm_battle_on {partymode_mgr is_mode_included rhythm_battle})
               ($dance_battle_on {partymode_mgr is_mode_included dance_battle})
               ($strike_a_pose_on {partymode_mgr is_mode_included strike_a_pose})
               {partymode_mgr set_custom_party TRUE}
               {hamprovider set is_in_infinite_party_mode TRUE}
               {partymode_mgr setup_infinite_party_mode}
               {partymode_mgr toggle_included_mode_on
                  perform
                  {if_else $perform_on
                     1
                     0
                  }
               }
               {partymode_mgr toggle_included_mode_on
                  bustamove
                  {if_else $bustamove_on
                     1
                     0
                  }
               }
               {partymode_mgr toggle_included_mode_on
                  rhythm_battle
                  {if_else $rhythm_battle_on
                     1
                     0
                  }
               }
               {partymode_mgr toggle_included_mode_on
                  dance_battle
                  {if_else $dance_battle_on
                     1
                     0
                  }
               }
               {partymode_mgr toggle_included_mode_on
                  strike_a_pose
                  {if_else $strike_a_pose_on
                     1
                     0
                  }
               }
               {partymode_mgr set_modes}
               {partymode_mgr set curr_synced_song_id 0}
               {partymode_mgr set_custom_party FALSE}
            }
            {infinite_party_confirm_quit_screen set ok_screen {ui current_screen}}
            {infinite_party_confirmation_screen set back_screen {ui current_screen}}
            {ui goto_screen infinite_party_hub_screen}
         )
         (options
            {party_mode_options_screen set back_screen choose_mode_screen}
            {ui goto_screen party_mode_options_screen}
         )
         (crew_showdown
            {enter_party_mode}
            {ui goto_screen party_mode_welcome_screen}
         )
         #else
         (custom_party
            {enter_infinite_party_mode}
            {partymode_mgr set_custom_party TRUE}
            {infinite_party_confirm_quit_screen set ok_screen {ui current_screen}}
            {ui goto_screen infinite_party_mode_branch_screen}
         )
         (crew_showdown
            {if_else {&& {exists profile_mgr} {profile_mgr get_disable_photos}}
               {do
                  {enter_party_mode}
                  {partymode_mgr set_custom_party TRUE}
                  {ui goto_screen party_mode_check_enable_photos_screen}
               }
               {do
                  {enter_party_mode}
                  {partymode_mgr set_custom_party TRUE}
                  {ui goto_screen party_mode_branch_screen}
               }
            }
         )
         #endif
         (rtnbldrproto
            {gamemode set_mode routine_builder}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (namethatdance
            {gamemode set_mode name_that_dance}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (cascade
            {gamemode set_mode cascade}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (concentration
            {gamemode set_mode concentration}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (rhythm_battle
            {gamemode set_mode rhythm_battle}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (holla_back_70s_craze
            {gamemode set_mode holla_back}
            {hamprovider set holla_back_config holla_back_config_craze_70s}
            {ui goto_screen {gamemode get newsong_screen}}
         )
         (mind_control
            {setup_game_mode teachmehowtodougie
               dci
               ((kDifficultyMedium) (kDifficultyExpert))
               mind_control_non_campaign
            }
            {{gamedata player 0} set_character_outfit rasa06}
            {{gamedata player 1} set_character_outfit lima06}
            {ui goto_screen loading_screen}
         )
         (bustamove
            {gamemode set_mode bustamove}
            {ui goto_screen loading_screen}
         )
         (screen_name_main {ui go_back_screen {{ui current_screen} get back}})
         (back
            #ifdef DEMO

            #else
            {ui go_back_screen {{ui current_screen} get back}}
            #endif
         )
      }
   )
   (NAV_HIGHLIGHT_MSG
      #ifdef DEMO
      {description.lbl set text_token ""}
      #else
      {switch $name
         (perform
            {description.lbl set text_token choose_mode_description_perform_it}
         )
         (practice
            {description.lbl set text_token choose_mode_description_practice}
         )
         (dance_battle
            {description.lbl set text_token choose_mode_description_dance_battle}
         )
         (custom_party
            {description.lbl set text_token choose_mode_description_party_mode}
         )
         (crew_showdown
            {description.lbl set text_token choose_mode_description_crew_showdown}
         )
      }
      #endif
      #ifdef DEMO

      #else
      {if {== $component right_hand.hnl}
         {game_mode_icon set_mode $name}
         {game_mode_icon show}
      }
      #endif
   )
}
{new HamScreen
   choose_mode_screen
   (panels meta background_panel choose_mode_panel)
   (focus choose_mode_panel)
   #ifdef DEMO

   #else
   (back main_screen)
   #endif
   (enter)
   (on_voice_command
      ($command)
      {switch $command
         (screen_name_main {helpbar select_left_list_item 1})
         BACRO
         {choose_mode_panel select_right_list_token $command}
      }
   )
}
{new HamScreen
   leaderboards_need_to_sign_in_screen
   (panels meta background_confirmation_panel dialog_panel)
   (focus dialog_panel)
   (next_screen)
   (enter {dialog_panel set_ok leaderboards_noprofile FALSE})
   (NAV_SELECT_MSG {ui goto_screen [next_screen]})
}