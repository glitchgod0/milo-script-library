   (allowed_dirs)
   (editor
      (draw_group
         object
         (class Group)
         (help "Should contain everything to draw (except widget resources)")
      )
      (smasher_group
         object
         (class Group)
         (help "All smasher geometry that flies down the track")
      )
      (track_mesh
         object
         (class Mesh)
         (help "piece of geometry that makes up a segment of the track")
      )
      (y_per_beat_easy
         float
         (range 1 10000)
         (help "World units the smasher moves per beat on easy difficulty")
      )
      (y_per_beat_medium
         float
         (range 1 10000)
         (help "World units the smasher moves per beat on medium difficulty")
      )
      (y_per_beat_hard
         float
         (range 1 10000)
         (help "World units the smasher moves per beat on hard difficulty")
      )
      (y_per_beat_expert
         float
         (range 1 10000)
         (help "World units the smasher moves per beat on expert difficulty")
      )
      (smasher_offset_y float (help "Smasher offset in y to line up with beat"))
      (rot_per_segment
         float
         (help "Rotation per segment of track.  In degrees.")
      )
      (capture_time_delta
         float
         (help "Time between segments playing their capture effect")
      )
      (stop_dist
         float
         (help "How far it takes the camera to stop at the end of the song.")
      )
      (toggle_test
         script
         (script {$this toggle_test})
         (help "Toggle running the track in milo")
         (label "toggle")
      )
      (test_song
         symbol
         (help "Short name of the song to test.  Ex. bulletproof")
      )
      (test_start_beat
         int
         (help "The beat that we're going to start with for the test.")
      )
      (test_end_beat
         int
         (help "The beat that we'll pretend is the last beat of the song.")
      )
      (test_song_section
