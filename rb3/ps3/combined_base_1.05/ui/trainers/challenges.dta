{new
   TrainerChallenge
   deploy_challenge
   (enter
      {set
         [has_deployed]
         FALSE}
      {{beatmatch active_player 0}
         set_mercuryswitchenabled
         TRUE}
      {{beatmatch active_player 0}
         add_sink
         $this})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this})
   (deploy
      {set
         [has_deployed]
         TRUE})
   (success
      [has_deployed])
   (restriction_token tc_deploy)
   (has_deployed FALSE)}
{new
   TrainerChallenge
   bre_challenge
   (enter
      {set
         [has_hit_bre]
         FALSE}
      {{beatmatch active_player 0}
         add_sink
         $this})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this})
   (finished_coda
      {set
