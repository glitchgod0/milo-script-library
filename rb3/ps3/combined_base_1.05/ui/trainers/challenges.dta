{new
   TrainerChallenge
   sustain_challenge
   (enter
      {{beatmatch active_player 0}
         add_sink
         $this}
      {set
         [failed]
         FALSE})
   (exit
      {{beatmatch active_player 0}
         remove_sink
         $this})
   (held_note_released_callback
      ($fraction)
      {if
         {< $fraction 1.0}
         {set
            [failed]
            TRUE}})
   (success
      {!
         [failed]})
   (restriction_token tc_sustain)
   (failed FALSE)}