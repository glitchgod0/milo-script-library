{new
   UIPanel
   tour_welcome_panel
   (file
      "tour_welcome.milo")
   (focus
      "continue.btn")
   (enter
      {if_else
         {is_leader_local}
         {continue.btn set_showing TRUE}
         {continue.btn set_showing FALSE}}
      {description.lbl
         set_token_fmt
         {{tour progress}
            get_tour_welcome}
         ((name
               {{tour progress}
                  get_tour_name}))})
   (SELECT_MSG
      {switch
         $component
         (continue.btn
            {ui goto_screen tour_gigguide_init_screen})})}
{new
   BandScreen
   tour_welcome_screen
   (panels meta sv4_panel tour_welcome_panel)
   (focus tour_welcome_panel)
   (back tour_customize_screen)}