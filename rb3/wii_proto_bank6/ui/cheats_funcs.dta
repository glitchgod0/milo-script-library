{func
   next_character_driver
   {if
      $world
      {do
         ($index -1)
         {foreach
            $obj
            (CHAR_OBJECTS)
            {if
               {==
                  $char_debug_driver
                  {'++' $index}}
               {unless
                  {char_debug $obj}
                  {'++' $char_debug_driver}}}}
         {if
            {==
               $char_debug_driver
               {'++' $index}}
            {char_debug}}
         {set
            $char_debug_driver
            {mod
               {'+' 1 $char_debug_driver}
               {'++' $index}}}}}}
