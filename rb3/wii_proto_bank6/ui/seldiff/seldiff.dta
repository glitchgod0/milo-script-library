{new UIPanel
   seldiff_border_panel
   (enter
      {if_else {== {meta_performer num_songs} 1}
         {do
            {song.lbl set_song_name {meta_performer song}}
            {by_line.lbl set text_token ''}
         }
         {do
            {song.lbl set text_token set_list_title}
            {by_line.lbl set_token_fmt
               set_list_song_count
               {localize_separated_int {meta_performer num_songs}}
            }
         }
      }
   )
}
{new UIPanel
   h2h_seldiff_panel
   (enter
      {player.lbl set_showing 0}
      {waiting_part.lbl set_showing 0}
      {ready.lbl set_showing 0}
      {do
         ($leader {session_mgr get_leader_user})
         {$this set_focus
            {if_else {== {$leader get_track_type} 8}
               guitar.btn
               {sprint {$leader get_difficulty_sym} ".btn"}
            }
         }
      }
      {session add_sink $this}
      {$this update}
   )
   (exit {session remove_sink $this})
   (REMOTE_USER_UPDATED_MSG {$this update})
   (update
      {do
         ($leader {session_mgr get_leader_user})
         ($has_part {!= {$leader get_track_type} 8})
         {selpart.grp set_showing {! $has_part}}
         {seldiff.grp set_showing $has_part}
         {if $has_part
            {instrument.lbl set_icon {cnv_instrumenttoicon {$leader get_track_sym}}}
         }
      }
      {foreach $part (guitar bass)
         {do
            ($button {sprint $part ".btn"})
            ($label {sprint $part ".lbl"})
            {if_else {song_mgr part_plays_in_song {meta_performer song} $part}
               {do
                  {$this enable $button}
                  {$this enable $label}
               }
               {do
                  {$this disable $button}
                  {$this disable $label}
               }
            }
         }
      }
   )
   (BUTTON_DOWN_MSG
      {do
         ($leader {session_mgr get_leader_user})
         {if_else
            {&&
               {== $action 2}
               {!= {$leader get_track_type} 8}
               {== {$leader get_controller_type} 1}
            }
            {do
               {meta send_back_sound_msg_to_all}
               {play_instr_sfx $user button_back}
               {$this set_focus {sprint {$leader get_track_sym} ".btn"}}
               {$this update_track none}
               {$this update}
            }
            kDataUnhandled
         }
      }
   )
   (update_track
      ($track)
      {user_mgr foreach_user $user
         {if {$user is_participating}
            {if_else {$user is_local}
               {$user set_track_type $track}
               {gamecfg set_remote_user_track_type $user $track}
            }
         }
      }
   )
   (SELECT_MSG
      {do
         ($leader {session_mgr get_leader_user})
         {if_else {== {$leader get_track_type} 8}
            {do
               ($track {basename {$component name}})
               {$this set_focus {sprint {$leader get_difficulty_sym} ".btn"}}
               {$this update_track $track}
               {$this update}
            }
            {do
               ($diff {basename {$component name}})
               {user_mgr foreach_user $part_user
                  {if {$part_user is_participating}
                     {if_else {$part_user is_local}
                        {$part_user set_difficulty $diff}
                        {gamecfg set_remote_user_difficulty $part_user $diff}
                     }
                  }
               }
               {ui goto_screen preloading_screen}
            }
         }
      }
      kDataUnhandled
   )
}
{new BandScreen
   h2h_seldiff_screen
   (panels meta postsong_sfx_panel h2h_seldiff_panel seldiff_border_panel)
   (focus h2h_seldiff_panel)
   (back song_select_screen)
}
{new UIPanel
   h2h_ranked_preload_panel
   (enter
      {if {is_leader_local}
         {meta_performer set_song {song_mgr get_random_song}}
         {meta_performer set_venue {random_venue}}
      }
      {user_mgr foreach_user $user
         {if {&& {$user is_participating} {$user is_local}}
            {$user set_difficulty {main_h2h_seldiff_panel get difficulty}}
         }
      }
      {if {== {session num_users} 2}
         {ui goto_screen preloading_screen}
      }
   )
}
{new BandScreen
   h2h_ranked_preload_screen
   (panels meta h2h_ranked_preload_panel)
   (focus h2h_ranked_preload_panel)
}
{new BandScreen
   jukebox_seldiff_screen
   (panels meta)
   (enter
      {do
         ($user {session_mgr get_leader_user})
         {switch {$user get_controller_type}
            (0 {$user set_track_type 0})
            (2 {$user set_track_type 3})
            {$user set_track_type 1}
         }
         {ui goto_screen preloading_screen}
      }
   )
}
{new SelectDifficultyPanel
   part_difficulty_panel
   (file seldiff.milo)
   (marquee_rotation_ms 3000)
   (enter
      {if_else {== {meta_performer num_songs} 1}
         {song_count.lbl set text_token 1_song}
         {song_count.lbl set_token_fmt
            song_select_song_count
            {localize_separated_int {meta_performer num_songs}}
         }
      }
   )
   (update_setlist_label
      ($token)
      {setlist_title.lbl set text_token $token}
   )
   (update_preview_song
      ($random_setlist $song $album_art)
      {if_else $random_setlist
         {marquee.grp set_showing 0}
         {do
            {marquee.grp set_showing 1}
            {coming_up.lbl set text_token coming_up}
            {song_preview.lbl set_song_name $song}
            {artist_preview.lbl set_artist_name_from_shortname $song}
            {if_else {! {== $album_art ""}}
               {album_art.pic set tex_file $album_art}
               {album_art.pic set tex_file "ui/image/blank_album_art_keep.png"}
            }
         }
      }
   )
}
{new BandScreen
   part_difficulty_screen
   (panels
      meta
      sv4_panel
      part_difficulty_panel
      (song_select_panel
         (active 0)
         (always_load 0)
      )
   )
   (enter
      {overshell add_sink $this}
      {overshell begin_override_flow 1}
      {if {&& {! {gamemode in_mode practice}} {session_mgr is_leader_local}}
         {meta_performer select_random_venue}
      }
   )
   (exit {overshell remove_sink $this})
   (override_ended
      ($override_type $cancel)
      {if {&& {== $override_type 1} {is_leader_local}}
         {if_else $cancel
            {do
               {cond
                  ({gamemode in_mode tour}
                     {ui goto_screen {get_tour_screen}}
                  )
                  ({gamemode in_mode qp_coop}
                     {ui goto_screen song_select_screen}
                  )
                  ({gamemode in_mode practice}
                     {gamemode set_mode qp_coop}
                     {ui goto_screen song_select_screen}
                  )
                  ({gamemode in_mode party_shuffle}
                     {ui goto_screen main_hub_screen}
                  )
                  ({gamemode in_mode campaign}
                     {ui goto_screen accomplishment_screen}
                  )
                  {ui goto_screen main_hub_screen}
               }
               {meta_performer clear_venues}
            }
            {if_else {gamemode in_mode practice}
               {ui goto_screen practice_preloading_screen}
               {ui goto_screen preloading_screen}
            }
         }
      }
   )
}