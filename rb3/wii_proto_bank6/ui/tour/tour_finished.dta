{new UIPanel
   tour_finished_panel
   (file "tour_finished.milo")
   (focus "continue.btn")
   (enter
      {total.sd set_values
         {{tour progress} get_num_stars}
         {{tour progress} get_total_stars_for_tour}
      }
      {status.lbl set_token_fmt
         tour_finished_status
         {{tour progress} get_tour_status 0}
      }
      {if_else {{tour progress} earned_encore}
         {if_else {{tour progress} won_encore}
            {encore_status.lbl set text_token tour_finished_won_encore}
            {encore_status.lbl set text_token tour_finished_lost_encore}
         }
         {encore_status.lbl set text_token tour_finished_no_encore}
      }
      {if_else {meta_performer get_band}
         {band_status.lbl set_band_name {meta_performer get_band}}
         {band_status.lbl set text_token tour_no_band}
      }
      {band_stars.lbl set_token_fmt tour_stars {{tour progress} get_num_stars}}
      {if_else {{tour progress} does_tour_status_exist -1}
         {do
            {status1.lbl set text_token {{tour progress} get_tour_status -1}}
            {status1_stars.lbl set_token_fmt
               tour_stars
               {{tour progress} get_num_stars_for_tour_status -1}
            }
         }
         {do
            {status1.lbl set text_token ''}
            {status1_stars.lbl set text_token ''}
         }
      }
      {if_else {{tour progress} does_tour_status_exist 0}
         {do
            {status2.lbl set text_token {{tour progress} get_tour_status 0}}
            {status2_stars.lbl set_token_fmt
               tour_stars
               {{tour progress} get_num_stars_for_tour_status 0}
            }
         }
         {do
            {status2.lbl set text_token ''}
            {status2_stars.lbl set text_token ''}
         }
      }
      {if_else {{tour progress} does_tour_status_exist 1}
         {do
            {status3.lbl set text_token {{tour progress} get_tour_status 1}}
            {status3_stars.lbl set_token_fmt
               tour_stars
               {{tour progress} get_num_stars_for_tour_status 1}
            }
         }
         {do
            {status3.lbl set text_token ''}
            {status3_stars.lbl set text_token ''}
         }
      }
      {if_else {{tour progress} does_tour_status_exist 2}
         {do
            {status4.lbl set text_token {{tour progress} get_tour_status 2}}
            {status4_stars.lbl set_token_fmt
               tour_stars
               {{tour progress} get_num_stars_for_tour_status 2}
            }
         }
         {do
            {status4.lbl set text_token ''}
            {status4_stars.lbl set text_token ''}
         }
      }
   )
   (SELECT_MSG {ui goto_screen tour_customize_screen})
}
{new BandScreen
   tour_finished_screen
   (panels meta tour_finished_panel)
   (focus tour_finished_panel)
   (back main_hub_screen)
}