{new
   UIPanel
   tour_encore_panel
   (file
      "tour_encore.milo")
   (focus
      "continue.btn")
   (enter
      {{tour progress}
         set_playing_encore}
      {total.sd
         set_values
         {{tour progress}
            get_num_stars}
         {{tour progress}
            get_total_stars_for_tour}}
      {tour clear_current_quest})
   (SELECT_MSG
      {tour
         launch_filter
         {tour get_num_encore_songs}
         {tour get_encore_filter}
         {tour get_encore_is_random}
         song_select_screen
         tour_part_difficulty_screen
         tour_encore_screen})
   #ifndef _SHIP
   (cheat_win_encore
      {{tour progress}
         set_won_encore
         1})
   #endif}
{new
   BandScreen
   tour_encore_screen
   (panels meta tour_encore_panel)
   (focus tour_encore_panel)
   (back main_hub_screen)}