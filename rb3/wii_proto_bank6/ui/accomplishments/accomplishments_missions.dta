{new AccomplishmentMissionPanel
   accomplishment_mission_panel
   (file "accomplishment_missions.milo")
   (focus "missions.lst")
   (initially_selected_mission '')
   (SELECT_MSG
      {accomplishment_panel set_mission {$this get_selected_mission}}
      {ui goto_screen accomplishment_screen}
   )
   (BUTTON_DOWN_MSG
      {switch $action
         (1
            {if_else {$this is_empty}
               0
               kDataUnhandled
            }
         )
         kDataUnhandled
      }
   )
   (refresh {accomplishments_status_panel refresh})
   (update_mission_provider
      ($provider)
      {missions.lst set_provider $provider}
   )
   (get_selected_mission_index {missions.lst selected_pos})
   (set_selected_mission_index
      ($index)
      {missions.lst set_selected $index}
   )
   (get_initially_selected_mission [initially_selected_mission])
   (clear_initially_selected_mission
      {$this set initially_selected_mission ''}
   )
}
{new BandScreen
   accomplishments_mission_screen
   (panels
      meta
      sv7_panel
      accomplishments_status_panel
      accomplishment_mission_panel
   )
   (focus accomplishment_mission_panel)
   (back accomplishments_hub_screen)
}