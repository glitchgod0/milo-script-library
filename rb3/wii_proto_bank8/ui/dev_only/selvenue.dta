{new
   UIPanel
   selvenue_panel
   (file
      "selvenue.milo")
   (focus venue.lst)
   (enter
      {do
         ($venue_array
            {array
               (VENUES VENUES_VIDEO)})
         {insert_elem $venue_array 0 no_venue_override}
         {venue.lst set_data $venue_array}
         {$this update_venue_selection}})
   (update_venue_selection
      {venue.lst
         set_selected
         {meta_performer get_venue_override}})
   (SELECT_MSG
      {meta_performer
         set_venue_override
         {venue.lst selected_sym}}
      {ui pop_screen})}
{new
   BandScreen
   selvenue_screen
   (panels selvenue_panel)
   (focus selvenue_panel)}