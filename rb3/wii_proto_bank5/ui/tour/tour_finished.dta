{new UIPanel
   tour_finished_panel
   (file "tour_finished.milo")
   (focus "continue.btn")
   (enter
      {total.sd set_values
         {{tour progress} get_num_stars}
         {{tour progress} get_total_stars_for_tour}
      }
      {conclusion.lbl set text_token {tour get_tour_conclusion_text}}
      {leaderboard.ihp set_showing
         {&& {{tour progress} does_tour_have_leaderboard} {session_mgr is_local}}
      }
      {if_else {tour has_bronze_medal}
         {bronze.mesh set mat prize.mat}
         {bronze.mesh set mat prize_gray.mat}
      }
      {if_else {tour has_silver_medal}
         {silver.mesh set mat prize.mat}
         {silver.mesh set mat prize_gray.mat}
      }
      {if_else {tour has_gold_medal}
         {gold.mesh set mat prize.mat}
         {gold.mesh set mat prize_gray.mat}
      }
   )
   (BUTTON_DOWN_MSG
      {switch $action
         (14
            {if {&& {{tour progress} does_tour_have_leaderboard} {session_mgr is_local}}
               {campaign_goals_leaderboard_panel set_goal
                  {{tour progress} get_tour_leaderboard_goal}
               }
               {tour_goals_leaderboard_screen set return_screen tour_finished_screen}
               {ui goto_screen tour_goals_leaderboard_screen}
            }
            'kDataUnhandled;'
         )
         kDataUnhandled
      }
   )
   (SELECT_MSG {ui goto_screen meta_loading_continue_screen})
}
{new BandScreen
   tour_finished_screen
   (panels meta tour_finished_panel)
   (focus tour_finished_panel)
   (back main_hub_screen)
}
{new UIPanel
   tour_goals_leaderboard_header_panel
   (file "tour_leaderboard_header.milo")
}
{new BandScreen
   tour_goals_leaderboard_screen
   (panels
      meta
      tour_goals_leaderboard_header_panel
      campaign_goals_leaderboard_panel
   )
   (focus campaign_goals_leaderboard_panel)
   (return_screen '')
   (BUTTON_DOWN_MSG
      {switch $action
         (2
            {ui goto_screen [return_screen]}
            {$this set return_screen ''}
         )
         kDataUnhandled
      }
   )
}