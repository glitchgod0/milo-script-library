(netcache_init
   (xlsp_filter "Harmonix")
   #ifdef HX_XBOX
      (servers
         (live
            (server "")
            (port 1000)
            (root "/turbo/xbox/live/")
            (verify_ssl TRUE)
         )
         #ifndef _SHIP
         (prelive
            (server "")
            (port 1000)
            (root "/turbo/xbox/prelive/")
            (verify_ssl TRUE)
         )
         (test
            (server "")
            (port 1000)
            (root "/turbo/xbox/test/")
            (debug TRUE)
            (verify_ssl TRUE)
         )
         (test_server_down
            (server "")
            (port 1000)
            (root "/turbo/xbox/fail/")
            (verify_ssl TRUE)
         )
         (local
            (local TRUE)
            (root "ui/store/remote_metadata/")
         )
         #endif
      )
      #ifdef _SHIP
      (default_server live)
      #else
      (default_server prelive)
      #endif
   #endif
   #ifndef HX_XBOX
   (servers
      (local
         (local TRUE)
         (root "ui/store/remote_metadata/")
      )
   )
   (default_server local)
   #endif
)
(dummy_upsell_offer
   (type song)
   (name {localize unknown_song})
)
(groupings
   (by_release_date
      (browser_group
         ($offer)
         {$offer release_date_str}
      )
      (localize_heading FALSE)
   )
   (by_review
      (browser_group
         ($offer)
         {sprintf "review_%i" {int {$offer get_data (review)}}}
      )
      (localize_heading TRUE)
   )
   (by_song_first_letter
      (browser_group
         ($offer)
         {$offer first_char name TRUE}
      )
      (localize_heading FALSE)
   )
   (by_artist
      (browser_group
         ($offer)
         {toupper {$offer get_data (artist)}}
      )
      (localize_heading FALSE)
   )
   (by_difficulty
      (browser_group
         ($offer)
         {song_mgr rank_tier_token
            {song_mgr rank_tier {$offer get_data (rank band)} band}
         }
      )
      (localize_heading TRUE)
   )
   (by_year_released
      (browser_group
         ($offer)
         {sprintf "%i" {$offer year_released}}
      )
      (localize_heading FALSE)
   )
   (by_pack_first_letter
      (browser_group
         ($offer)
         {$offer pack_first_letter}
      )
      (localize_heading FALSE)
   )
   (by_author
      (browser_group
         ($offer)
         {$offer get_data (author)}
      )
      (localize_heading FALSE)
   )
   (by_label
      (browser_group
         ($offer)
         {$offer get_data (label)}
      )
      (localize_heading FALSE)
   )
   (by_subgenre
      (browser_group
         ($offer)
         {$offer get_data (subgenre)}
      )
      (localize_heading TRUE)
   )
)