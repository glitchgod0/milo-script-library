(Mat
   (description
      "Material objects determine texturing, blending, and the\n        effect of lighting on drawn polys."
   )
   (ext mat)
   (types)
   (metamaterial_path "config")
   (editor
      (metamaterial
         object
         (class MetaMaterial)
         (dir {$this get_metamats_dir})
         (list
            {$this get_metamats
               {&& milo {has_substr {milo current_file} "system\\run"}}
            }
         )
         (help "MetaMaterials set the Material properties to be edited")
         (no_test)
         (refresh)
      )
      (toggle_display_all_properties
         script
         (script {$this toggle_display_all_props})
         (hide {== [metamaterial] ''})
         (alias "Toggle the display of all properties")
         (refresh)
         (help
            "Show all properties set to default or force status in the MetaMaterial"
         )
      )
      (color
         color
         (help "Base material color")
         (hide {$this prop_is_hidden color})
         (read_only {$this prop_is_read_only color})
      )
      (alpha
         float
         (help "Base material alpha")
         (hide {$this prop_is_hidden alpha})
         (read_only {$this prop_is_read_only alpha})
      )
      (use_environ
         bool
         (help
            "Modulate with environment ambient and lights, will disable vertex color but not alpha"
         )
         (hide {$this prop_is_hidden use_environ})
         (read_only {$this prop_is_read_only use_environ})
      )
      (prelit
         bool
         (help "Use vertex color and alpha for the base or ambient color")
         (hide {$this prop_is_hidden prelit})
         (read_only {$this prop_is_read_only prelit})
      )
      (diffuse_tex
         object
         (class Tex)
         (help "Base texture map, modulated with color and alpha")
         (refresh intensify)
         (hide {$this prop_is_hidden diffuse_tex})
         (read_only {$this prop_is_read_only diffuse_tex})
      )
      (intensify
         bool
         (help "Double the intensity of base map")
         (hide {$this prop_is_hidden intensify})
         (read_only {'||' {! [diffuse_tex]} {$this prop_is_read_only intensify}})
      )
      (tex_wrap
         int
         (list TEXWRAP_ENUM)
         (help "Texture mapping mode")
         (hide {$this prop_is_hidden tex_wrap})
         (read_only {$this prop_is_read_only tex_wrap})
      )
      (tex_gen
         int
         (list TEXGEN_ENUM)
         (help "How to generate texture coordinates")
         (hide {$this prop_is_hidden tex_gen})
         (read_only {$this prop_is_read_only tex_gen})
      )
      (tex_xfm
         TRANSFORM
         (help "Transform for coordinate generation")
         (hide {$this prop_is_hidden tex_xfm})
         (read_only {$this prop_is_read_only tex_xfm})
      )
      (blend
         int
         (list BLEND_ENUM)
         (help "How to blend poly into screen")
         (hide {$this prop_is_hidden blend})
         (read_only {$this prop_is_read_only blend})
         (refresh misc_settings)
      )
      (z_mode
         int
         (list ZMODE_ENUM)
         (help "How to read and write z-buffer")
         (hide {$this prop_is_hidden z_mode})
         (read_only {$this prop_is_read_only z_mode})
      )
      (alpha_cut
         bool
         (help "Cut zero alpha pixels from z-buffer")
         (refresh alpha_threshold misc_settings)
         (hide {$this prop_is_hidden alpha_cut})
         (read_only {$this prop_is_read_only alpha_cut})
