(Anim
   (description
      "Base class for animatable objects. Anim objects change\n        their state or other objects."
   )
   (allowed_dirs)
   (editor
      (rate
         int
         (list RATE_ENUM)
         (help "Rate to animate")
      )
      (frame
         float
         (help "Frame of animation")
         (range 0 {$this end_frame})
         (no_test)
      )
   )
   (views
      (animating
         {$this is_animating}
         ("current frame" {$this frame})
      )
   )
)
(Poll
   (description "Abstract base class for pollable RND objects")
   (allowed_dirs)
   (editor
      (enter script (script {$this enter}))
      (exit script (script {$this exit}))
   )
   (types)
)
(Enterable
   (description "A simple object with an enter an exit script call")
