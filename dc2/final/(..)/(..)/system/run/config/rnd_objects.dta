)
(Cam
   (description
      "A Camera object is drawable and transformable. When drawn\n                 it sets up projection and clipping parameters for\n                 subsequent draw siblings."
   )
   (superclasses Trans)
   (ext cam)
   (types)
   (editor
      (near_plane
         float
         (help
            "The distance in world coordinates to the near clipping\n            plane. The near/far ratio is limited to 1:1000 to preserve\n            Z-buffer resolution."
         )
         (range
            {/ [far_plane] 1000.0}
            [far_plane]
         )
      )
      (far_plane
         float
         (help
            "The distance in world coordinates to the far clipping\n            plane. The near/far ratio is limited to 1:1000 to preserve\n            Z-buffer resolution. Note that on the PS2, object polys are\n            culled rather than clipped to the far plane."
         )
         (range
            [near_plane]
            {'*' [near_plane] 1000.0}
         )
      )
      (y_fov float)
      (z_range
         VECTOR2
         (help
            "The part of the Z-buffer to use, in normalized\n            coordinates. It can be useful to draw a scene where the near\n            and far planes must exceed the 1:1000 ratio (so multiple\n            cameras are used to draw farthest to nearest objects, each\n            using a closer range of the z-buffer) or to leave some\n            z-buffer for HUD overlay objects."
         )
      )
      (screen_rect
         RECT
         (help
            "The area of the screen in normalized coordinates\n            (0 to 1) to draw into."
         )
      )
      (set_milo_cam script (script {milo set cur_cam $this}))
      (set_tool_cam
         script
         (script
            {milo set tool_cam $this}
            {milo update_objects TRUE}
         )
         (hide {== {milo get tool_cam} $this})
         (help "Applies this cam as the tool view.")
      )
      (reset_tool_cam
         script
         (script
            {milo set tool_cam ""}
            {milo update_objects TRUE}
         )
         (hide {== {milo get tool_cam} ""})
         (help "Resets the tool view to be same as Milo's view.")
      )
   )
)
(CamAnim
   (description "A CamAnim object animates Camera properties.")
   (superclasses Anim)
   (ext cnm)
   (types)
   (editor
      (cam
         object
         (class Cam)
         (help "The camera whose fov we will animate")
      )
      (fov_keys
         (array
            (struct
               (frame
                  float
                  (help "The frame")
                  (refresh)
               )
               (value float (help "The fov to set, not sure about units"))
            )
            (header
               {do
                  ($index {elem $milo_prop_path 1})
                  {sprint "frame: " [fov_keys $index frame] " fov: " [fov_keys $index value]}
               }
            )
         )
         (help "The frames and corresponding fovs")
      )
      (keys_owner
         object
         (class CamAnim)
         (help "Owner of the keys, usually myself")
         (obj_flags no_null)
         (no_test)
      )
   )
)
(SoftParticles
