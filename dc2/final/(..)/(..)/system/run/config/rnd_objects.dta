         (no_test)
         (refresh mutable keep_mesh_data num_verts num_faces volume)
      )
      (mutable
         (struct
            (BIT_kMutableVerts
               bool
               (help "Verts are mutable for this mesh")
               (alias verts)
            )
            (BIT_kMutableFaces
               bool
