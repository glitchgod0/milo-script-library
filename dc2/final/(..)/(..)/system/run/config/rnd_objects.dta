   (hide
      {do
         ($i {elem $milo_prop_path {- {size $milo_prop_path} 2}})
         {! {&& [styles $i font_color_override] [styles $i font]}}
      }
   )
)
#define TEXT_FIT_TYPES (
   ("kFitWrap" "Performs normal line wrapping if [width] is set")
   (
      "kFitJust"
      "Shrinks the text until it fits within [width] and [height].  Note that this is a very expensive process, super slow, and so should never be used on dynamically changing text when in game"
   )
   ("kFitEllipsis" "Constrains the text to one line of [width] with ellipses")
)
#define TEXT_ALIGNMENTS (
   "kTopLeft"
   "kTopCenter"
   "kTopRight"
   "kMiddleLeft"
   "kMiddleCenter"
   "kMiddleRight"
   "kBottomLeft"
   "kBottomCenter"
   "kBottomRight"
)
#define TEXT_CAPS_MODES (
   ("kCapsModeNone" "Leave the text as is")
   ("kForceLower" "Force text to all lower case")
   ("kForceUpper" "Force text to all upper case")
)
(Text
   (description "A Text object draws a 3D character string.")
   (superclasses Draw Trans)
   (ext txt)
   (types)
   (editor
      (text
         string
         (help "Text value")
         (no_test)
         RNDTEXT_POST
      )
      (align
         int
         (list (TEXT_ALIGNMENTS))
         (help "Alignment option for the text")
         RNDTEXT_POST
      )
      (caps_mode
         int
         (list (TEXT_CAPS_MODES))
         (help "Defines the CAPS mode for the text")
         RNDTEXT_POST
      )
      (width
         float
         (range 0 10000)
         (help "Width of text until it wraps")
         RNDTEXT_POST
      )
      (height
         float
         (range 0 1000)
         (help "Height of the text, used for [fit_type] kFitJust")
         RNDTEXT_POST
      )
      (leading
         float
         (range -5 5)
         (help "Vertical distance between lines")
         RNDTEXT_POST
      )
      (fixed_length
         int
         (range 0 500)
         (help
            "Number of character maximum for the text, if non-zero makes underlying mesh mutable, so updates are faster"
         )
         RNDTEXT_POST
      )
      (markup
         bool
         (help
            "Support markup or not.  In the text, use <alt>, <alt2>, <alt3>, etc to use the higher styles, <sup> to get a super script, <nobreak> for preventing linebreaks in a block, <it> for italics, <gtr> for Bryn's guitar chord formatting.  Example: Hit <it>Back</it> <alt>B</alt> to continue<sup>TM</sup> "
         )
         RNDTEXT_POST
      )
      (fit_type
         int
         (list (TEXT_FIT_TYPES))
         (help "How to fit text in the width/height specified")
         RNDTEXT_POST
      )
      (styles
         (array
            (struct
               (font
                  object
                  (class Font)
                  (help "Font to use for this style")
                  (no_test)
                  (refresh styles)
                  RNDTEXT_POST
               )
               (size float (help "Size of the text") RNDTEXT_POST)
               (text_color
                  color
                  (help
                     "Color of the text, put into mesh verts.  Modifed by <color=r,g,b,a> markup .  This will only work if the font mat has [prelit] set true and [use_environment] set false"
                  )
                  RNDTEXT_POST
               )
               (text_alpha
                  float
                  (range 0 1)
                  (help
                     "Alpha value of the text, put into mesh verts.  This will only appear if the font mat has [prelit] set true"
                  )
                  RNDTEXT_POST
               )
               (font_color_override
                  bool
                  (help
                     "If true, and if there's a font, you can change color and alpha during the draw"
                  )
                  (refresh styles)
                  RNDTEXT_POST
               )
               (font_color
                  color
                  (help "Color of the font during draw, can be changed dynamically")
                  RNDTEXT_FONT_HIDE
                  RNDTEXT_POST
               )
               (font_alpha
                  float
                  (range 0 1)
                  (help "Alpha value of the font, can be changed dynamically")
                  RNDTEXT_FONT_HIDE
                  RNDTEXT_POST
               )
               (italics
                  float
                  (range -5 5)
                  (help "Defines the slant of the text, changed by <it> tag")
                  RNDTEXT_POST
               )
               (kerning float (help "Extra kerning for the text") RNDTEXT_POST)
               (z_offset float (help "vertical offset as fraction of size") RNDTEXT_POST)
            )
         )
         (help "The different styles this text can have")
      )
   )
)
